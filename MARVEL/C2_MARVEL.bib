@article{ 98RoLaMoCl,
Author = {Rousselot, P and Laffont, C and Moreels, G and Clairemidi, J},
Title = {{An attempt to detect the C$_2$ intercombination transition lines in comet
   Hale-Bopp}},
Journal = AA,
Year = {{1998}},
Volume = {{335}},
Pages = {765-768},
Abstract = {{In this paper we report the first attempt to detect directly the
   intercombination transition lines of the C$_2$ molecule in a comet.
   Indeed, these transitions play a key role in the C$_2$ fluorescence
   process but had never been observed directly so far. We conducted an
   observational program with the Cerro Tololo Interamerican Observatory 4
   m telescope, located in Chile, on September 15(th) and 16(th), 1997. The
   spectral range covered was 1.99 - 2.15 mu m, in order to detect the
   emission lines of the X(1)Sigma(g)(+)-a(3)II(u) (3,0) band, that should
   be degraded to the blue with a bandhead located at 2.127 mu m. This band
   was chosen because of its expected brightness and its wavelength,
   corresponding to a good atmospheric transmission and a relatively low
   sky background. In this paper we present the theoretical calculation
   allowing to model the expected spectrum and the results obtained with
   our observational data. These results, unfortunately, cannot allow the
   detection of the expected emission lines. This is due to a too strong
   noise, generated by the sky background emission lines, that cannot be
   totally removed from the spectrum.}}}

@article{83PoRoScRo,
Author = {B Pouilly and J M Robbe and J Schamps and E Roueff},
Title = {{Photodissociation and radiative processes in interstellar C$_2$}},
Journal =JPBX,
Year = {{1983}},
Volume = {16},
Pages = {437-448},
Abstract = {{Ab initio methods are used to compute photodissociation and radiative
processes occurring in interstellar C2 from the X 'Xi ground state.
To this end, self-consistent-field + configuration interaction
wavefunctions and potential energy curves of the X'X; and the first
four 'II, states and transition moments between the ground and excited
electronic states are computed. Photodissociation cross sections for
absorption into the second and third 'II, states are calculated and
the corresponding interstellar photodissociation probability is found
to be 0.73 x 10-'os-'.  Ground-state transition probabilities and
absorption oscillator strengths foo of the Phillips (A 'II,-X 'Xi) and
2 'II,-X 'Xi systems are determined.}}}

