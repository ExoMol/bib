@article{ 01BrGrxx.C2,
Author = {Bruna, P J and Grein, F},
Title = {{Spectroscopy of the C$_2$ molecule: Valence and Rydberg states in the 7-10
   eV region. An ab initio study}},
Journal = CJP,
Year = {{2001}},
Volume = {{79}},
Pages = {653-671},
Abstract = {{The potential curves of selected valence and Rydberg states of C$_2$ lying
   in the 7-10 eV region are reported for the first time. The states
   studied, using MRCI wave functions, include (2-5)(3,1)Pi (u),
   (1-3)(3)Sigma (+)(g), (2-5)(3)Sigma (g-), (1-3)(3)(g), and relevant
   quintet stares. The f (3)Sigmag(-), g(3)Deltag and F (1)Pi (u) states
   observed in absorption from 8.88 to 9.25 eV by Herzberg et al. have
   originally been assumed to be pi (u) --> 3s Rydberg states, with respect
   to a (3)Pi (u)(1 (3)Pi (u)) for the triplets and to X(1)Sigma (+)(g) for
   the singlet. Our calculations partially support such assignments: f
   (3)Sigma (-)(g) corresponding to 3 (3)Sigma (-)(g) has mixed valence
   sigma (u)sigma (g)pi (u) (3)pi (g) and Rydberg sigma (2)(u)sigma (g)pi
   (u) (2)3s character g(3)Delta (g) corresponding to 2(3)Delta (g) has a
   valence sigma (u)sigma (g)pi (u) (3)pi (g), structure, while F(1)Pi
   (u)(2(1)Pi (u)) is confirmed to be a sigma (u) (2)pi (u) (3)3s state.
   The calculated f(00)-values for these bands are 0.027 for f <-- a, 0.051
   for g <-- a, and 0.098 for F <-- X. The singlet-singlet transition F <--
   X constitutes the strongest absorption band reported so far for C$_2$. Our
   theoretical value agrees with f(00)(F <-- X) = 0.10 +/- 0.01 derived
   indirectly from interstellar data collected by the Hubble Space
   Telescope. The 1 (3) Sigma (+)(g) (sigma (u)sigma (g)pi (u) (3)pi (g))
   state at T-e = 7.57 eV lies much higher than expected by experimentalists.}},
DOI = {{10.1139/cjp-79-2-3-653}},
}

