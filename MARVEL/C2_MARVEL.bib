@article{88UrBaJa.C2,
title = {Observation of the {LIF} spectra of {C}$_2$(a 3Πu) and {C}$_2$(A 1Πu) from the photolysis of {C}$_2${H}$_2$ at 193 nm},
journal = CPL,
volume = "152",
pages = "485 - 490",
year = "1988",
doi = "http://dx.doi.org/10.1016/0009-2614(88)80446-9",
author = "R. S. Urdahl and Yihan Bao and William M. Jackson",
abstract = "The laser-induced fluorescence (LIF) spectra of the C2(d 3Πg)←C2(a 3Πu), Swan and C2(C 1Πg)←C2(A 1Πu), Deslandresd'Azambuja systems produced in the multiphoton photodissociation of \{C2H2\} have been observed. Preliminary analysis of the spectra shows that the nascent fragments are rotationally excited with J ⩽ 43 and J ⩽ 48 for the a 3Πu and A 1Πu states respectively. Time-dependent intensity measurements of the direct and the laser-induced emissions are used to estimate a lower limit for the nascent radical concentration ratios, [C2(a 3Πu)]/[C2(d 3Πg)] and [C2(A 1Πu)]/[C2(C 1Πg)]. The radiative lifetime of the \{C2\} (d 3Πg) state is τ(ν′=1) = 97.1±4.4 ns, in good agreement with a recently published result. "
}

@article{05SmPaSc,
title = "\{C2\} {S}wan band laser-induced fluorescence and chemiluminescence in low-pressure hydrocarbon flames ",
journal = "Combustion and Flame ",
volume = "141",
pages = "66 - 77",
year = "2005",
doi = "http://dx.doi.org/10.1016/j.combustflame.2004.12.010",
author = "Gregory P. Smith and Chung Park and Justin Schneiderman and Jorge Luque",
keywords = "Chemiluminescence",
keywords = "\{C2\}",
keywords = "\{LIF\}",
keywords = "Low-pressure flames ",
abstract = "Laser-induced fluorescence from the excitation of the C2(d-a) 2 , 0 band at 438 nm is used to determine spatial profiles and relative lower state concentrations in several low-pressure hydrocarbon–air premixed flames, to determine the fuel-rich flame kinetics. Quenching loss rate constants are derived from the measured fluorescence decay rates. Quantitative Swan band chemiluminescence intensities, coupled with computer modeling of the flame kinetics and excited state loss rates, lead to recommended values of rate constants for the excited C 2 ( d ) -state chemiluminescence production kinetics. "
}

@article{96BlLiSo.C2,
title = "\{LIF\} of \{C2\} (A 1Πu) via the Freymark band (E1Σ6+ ← A 1Πu) ",
journal = CPL,
volume = "257",
pages = "347 - 350",
year = "1996",
doi = "http://dx.doi.org/10.1016/0009-2614(96)00544-1",
author = "Victor M. Blunt and Hua Lin and Osman Sorkhabi and William M. Jackson",
abstract = "\{C2\} (A 1Πu) produced from multi-photon dissociation of \{C2H2\} at 193 nm was probed via the Δν = −2 sequence of the Freymark band system (EΣg+ ← A1πu) using laser induced fluorescence. The (ν′ = 0, ν″ = 2), (νv′ = 1, ν″ = 3) and a few lines of the (ν′ = 2, ν″ = 4) vibrational progressions were observed under nascent conditions. The intensity of the (ν′ = 0, ν″ = 2) and (ν′ = 1, ν″ = 3) bands are comparable but the (ν′ = 2, ν″ = 4) band is much weaker, implying that the vibrational population of the ν″ = 2 and 3 levels are approximately equal while that of ν″ = 4 is much smaller. "
}

