@article{ 05WaNiYe.CH4,
author = {J. Hunter Waite  and Hasso Niemann  and Roger V. Yelle  and Wayne T. Kasprzak  and Thomas E. Cravens  and Janet G. Luhmann  and Ralph L. McNutt  and Wing-Huen Ip  and David Gell  and Virginie De La Haye  and Ingo Müller-Wordag  and Brian Magee  and Nathan Borggren  and Steve Ledvina  and Greg Fletcher  and Erin Walter  and Ryan Miller  and Stefan Scherer  and Rob Thorpe  and Jing Xu  and Bruce Block  and Ken Arnett },
title = {Ion Neutral Mass Spectrometer Results from the First Flyby of Titan},
journal = {Science},
volume = {308},
number = {5724},
pages = {982-986},
year = {2005},
doi = {10.1126/science.1110652},
URL = {https://www.science.org/doi/abs/10.1126/science.1110652},
eprint = {https://www.science.org/doi/pdf/10.1126/science.1110652},
abstract = {The Cassini Ion Neutral Mass Spectrometer (INMS) has obtained the first in situ composition measurements of the neutral densities of molecular nitrogen, methane, molecular hydrogen, argon, and a host of stable carbon-nitrile compounds in Titan's upper atmosphere. INMS in situ mass spectrometry has also provided evidence for atmospheric waves in the upper atmosphere and the first direct measurements of isotopes of nitrogen, carbon, and argon, which reveal interesting clues about the evolution of the atmosphere. The bulk composition and thermal structure of the moon's upper atmosphere do not appear to have changed considerably since the Voyager 1 flyby.}}

