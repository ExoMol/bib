@article{09LyNiPe.CH4,
title = {Measurements of N2- and O2-broadening and shifting parameters of methane spectral lines in the 5550–6236cm−1 region},
journal = {Journal of Quantitative Spectroscopy and Radiative Transfer},
volume = {110},
number = {9},
pages = {654-668},
year = {2009},
note = {HITRAN},
issn = {0022-4073},
doi = {https://doi.org/10.1016/j.jqsrt.2009.02.012},
url = {https://www.sciencedirect.com/science/article/pii/S0022407309000752},
author = {O.M. Lyulin and A.V. Nikitin and V.I. Perevalov and Isamu Morino and Tatsuya Yokota and Ryoichi Kumazawa and Takeshi Watanabe},
keywords = {Molecular spectroscopy, Methane (CH), Spectral line parameters, N- and O-broadening and shifting, Temperature dependence of the broadening and shifting},
abstract = {The absorption spectra of mixtures of methane (CH4) with N2 and O2 at different partial pressures of both CH4 and buffer gases for three temperatures 240, 267, and 296K have been recorded using the Bruker IFS 125 HR FTIR spectrometer in the 5550–6236cm−1 region. The multispectrum fitting procedure has been applied to these spectra to recover the spectral line parameters. The main goal of this procedure was the determination of the N2- and O2-broadening and shifting coefficients and the exponents of their temperature dependences. These parameters have been derived for 452 assigned lines with good values of the signal to noise ratio. The rotational dependence of the mean values of these parameters is discussed. The temperature dependence exponents were observed for both N2 and O2 buffer gases.}
}

