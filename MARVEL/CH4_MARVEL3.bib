@article{20GiNaTu.CH4,
author = {Michele Gianella and Akshay Nataraj and B\'{e}la Tuzson and Pierre Jouy and Filippos Kapsalidis and Mattias Beck and Markus Mangold and andreas Hugi and J\'{e}r\^{o}me Faist and Lukas Emmenegger},
journal = {Optics Express},
keywords = {Discrete Fourier transforms; Femtosecond fiber lasers; Frequency combs; Laser beams; Laser sources; Quantum cascade semiconductor lasers},
pages = {6197--6208},
title = {High-resolution and gapless dual comb spectroscopy with current-tuned quantum cascade lasers},
volume = {28},
year = {2020},
doi = {10.1364/OE.379790},
abstract = {We present gapless, high-resolution absorption and dispersion spectra obtained with quantum cascade laser frequency combs covering 55 cm\&\#x2212;1. Using phase-sensitive dual comb design, the comb lines are gradually swept over 10 GHz, corresponding to the free spectral range of the laser Devices, by applying a current modulation. We show that with interleaving the spectral point spacing is reduced by more than four orders of magnitude over the full spectral span of the frequency comb. The potential of this technique for high-precision gas sensing is illustrated by measuring the low pressure (107 hPa) absorption and dispersion spectra of methane spanning the range of 1170 cm\&\#x2212;1 - 1225 cm\&\#x2212;1 with a resolution of 0.001 cm\&\#x2212;1.},
}

