Title = {{Line parameters of (CH4)-C-12 around 2.008 mu m studied by tunable diode
   laser spectroscopy with a long-path White cell}},
Journal = { Journal of the Optical Society of America B},
Year = {{2018}},
Volume = {{35}},
Pages = {2453-2458},
Abstract = {{The absorption spectrum of (CH4)-C-12 at 2.008 mu m has been recorded at
   296 K, using a high-resolution tunable diode laser absorption
   spectrometer combined with a long-path, multi-pass White cell. The line
   positions, intensities, and self-broadening coefficients for 32
   transitions of the lower part of the Tetradecad in the 4975-4985 cm(-1)
   spectral range of (CH4)-C-12 have been measured. To our knowledge, no
   experimental information has been obtained for the self-broadening
   coefficients of these lines before. Comparisons of the line parameters
   determined in the present work with those from HITRAN2016 line lists are
   provided. }},
DOI = {{10.1364/JOSAB.35.002453}},
}

