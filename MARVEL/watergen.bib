@article{ 12PeSoSo,
Author = {Petrova, T. M. and Solodov, A. M. and Solodov, A. A.},
Title = {{Line mixing in the water vapour transitions of the nu(1) + nu(2) + nu(3)
   band perturbed by helium pressure}},
Journal = MP,
Year = {{2012}},
Volume = {{110}},
Pages = {2071-2075},
Abstract = {{The line mixing effect for two pairs of transitions with rotational
   quantum numbers (3 1 2) <- (4 1 3), (3 2 1) <- (4 2 2), (4 3 1) <- (3 3
   0) and (4 2 2) <- (3 2 1) in the nu(1) + nu(2) + nu(3) band of H2O has
   been studied. The measurements were performed at room temperature, at
   the spectral resolution of 0.01 cm(-1) and in a wide pressure range of
   helium. Pressure-broadening and -shifting coefficients and mixing
   parameter were obtained with help of the line profile using the
   Rosenkranz first-order approximation.}},
