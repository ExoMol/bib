Title = {{Line mixing in self- and foreign-broadened water vapor at 6 mu m}},
Journal = JMS,
Year = {{2005}},
Volume = {{742}},
Pages = {111-122},
Abstract = {{Line mixing has been measured for two pairs of transitions in the v(2)
