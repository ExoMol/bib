@ARTICLE{91AmElEl.SO,
  author = {Amin, M Y and Elnawawy, M S and Elshalaby, M A},
  title = {{SHOCK-WAVES AND THE CHEMICAL-STRUCTURE OF INTERSTELLAR CLOUDS}},
