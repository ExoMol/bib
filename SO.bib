@ARTICLE{03WaLiCh.SO,
  author = {Wang, H M and Li, J Q and Chen, P and Zhang, W J and Cao, D Z and
	Chu, Y N},
  title = {{Spectroscopic study of SO(c(1)Sigma(-)) generated by the discharge
	of Ar and SO2 mixture}},
  journal = {Spectrosc. Spectr. Anal.},
  year = {2003},
  volume = {23},
  pages = {630-633},
  abstract = {In a flowing afterglow apparatus, the luminescence was observed during
	the hollow-cathode discharge of Ar and SO2 mixture. Besides emissions
	from SO(A(3)Pi-->X(3)Sigma(-)) and SO2 (a(3)B(1)-->X(1)A(1)), a rather
	strong progression in the range of 400-600 nm is assigned to SO(c(1)Sigma(-)-->X(3)Sigma(-))transition
	with nu(00) = 28 500 cm(-1), omega(e){''} = 1 155.5 cm(-1) and omega{''}(e)chi{''}(e)
	= 6.6 cm(-1). Another weak progression that was oberved in the range
	of 400-480 nm is ascribed to the emission from the nu' = 1 level
	of SO(c(1)Sigma(-)) state. Its separation by 673 cm(-1) from the
	main c(1)Sigma(-)-->X(3)Sigma(-) progression agrees with the previous
	Ar matrix experimental result. This is the first direct observation
	of the emission spectra of SO(c(1)Sigma(-)-->X(3)Sigma(-)) in gas
	phase, and provides the first gaseous spectroscopic information about
	the SO(c(1)Sigma(-)) state.},
  keywords = {Experiment}
}

@ARTICLE{05WaTaCh.SO,
