@article{ 94GoTaYa.AlO,
Author = {GOTO, M and TAKANO, S and YAMAMOTO, S and ITO, H and SAITO, S},
Title = {MICROWAVE-SPECTRA OF THE ALO (X(2)SIGMA+) RADICAL IN THE UPSILON=1 AND 2
   STATES},
Journal = CPL,
Year = {1994},
Volume = {227},
Number = {3},
Pages = {287-292},
Month = {SEP 9},
Abstract = {The microwave spectra of the aluminum monoxide radical (X 2SIGMA+) for v
   = 1 and 2 have been observed by using a source-modulated spectrometer
   combined with a free-space absorption cell. The Al atom was generated by
   discharge in pure N2O or Ar with an aluminum hollow-cathode containing
   several pieces of alumina. Five rotational transitions in the region
   180-380 GHz were measured. The fine and hyperfine structure patterns of
   a certain rotational transition showed that the spin-rotation coupling
   constant changes anomalously by vibrational excitation. The rotational
   constant, centrifugal distortion constant, spin-rotation coupling
   constant with its centrifugal distortion term, and hyperfine coupling
   constants for the Al nucleus were precisely determined for the v = 1 and
   2 states.},
DOI = {10.1016/0009-2614(94)00820-5},
ISSN = {0009-2614},
Times-Cited = {12},
Journal-ISO = {Chem. Phys. Lett.},
Unique-ID = {ISI:A1994PF45600010},
}

@article{ 94ItGoxx.AlO,
Author = {ITO, H and GOTO, M},
Title = {ANALYSIS OF AN ANOMALOUS VIBRATIONAL DEPENDENCE OF THE SPIN ROTATION
   CONSTANTS FOR ALO(X(2)SIGMA(+))},
Journal = CPL,
Year = {1994},
Volume = {227},
Number = {3},
Pages = {293-298},
Month = {SEP 9},
Abstract = {The spin-rotation constants, gamma(upsilon), for the upsilon = 0-2
   levels of AlO (X 2SIGMA+) were analyzed in terms of the X 2SIGMA+
   -approximately 2PI(i) and X 2SIGMA+ approximately C 2PI(r) second-order
   interactions. The X 2SIGMA+ approximately A 2 PI(i) spin-orbit and
   rotational-electronic interactions were determined as functions of the
   internuclear distance R. The anomalous vibrational dependence of
   gamma(upsilon) observed for upsilon = 1 and 2 was ascribed to the strong
   R dependence of the X 2SIGMA+ approximately A 2PI(i) spin-orbit
   interaction; the contributions of the X 2SIGMA+ approximately C 2PI(r)
   interactions were found to be nearly independent of upsilon. The R
   dependence of the X 2SIGMA+ approximately A 2PI(i) spin-orbit
   interaction was fully explained by the change in the electronic
   structure, Al2+O2- --> Al+O-, of X2SIGMA+.},
DOI = {10.1016/0009-2614(94)00821-3},
ISSN = {0009-2614},
Times-Cited = {8},
Journal-ISO = {Chem. Phys. Lett.},
Unique-ID = {ISI:A1994PF45600011},
}

@article{ 92CaMcGa.AlO,
Author = {CAMPBELL, ML and MCCLEAN, RE and GARLAND, NL and NELSON, HH},
Title = {ELECTRONIC QUENCHING OF THE B2-SIGMA+ STATE OF ALO},
Journal = CPL,
Year = {1992},
Volume = {194},
Number = {3},
Pages = {187-190},
Month = {JUN 26},
Abstract = {Electronic quenching rate constants for the B2-SIGMA+ state of AlO were
   determined using a pulsed-laser photolysis laser-induced fluorescence
   technique by examining fluorescence decay rates in the presence of seven
   atomic and molecular collision partners. The two polar molecules CO and
   NO were found to have rate constants of 4.8 x 10(-12) and 1.8 x 10(-10)
   cm3 s-1, respectively. The nonpolar gases He, H2, N2, CO2 and Xe were
   found to be poor quenchers with a bimolecular quenching rate constant
   having an upper bound of 1 X 10(-13) cm3 s-1 for He, H-2, N2 and Xe and
   1 x 10(-12) cm3 for CO2.},
DOI = {10.1016/0009-2614(92)85532-F},
ISSN = {0009-2614},
Times-Cited = {7},
Journal-ISO = {Chem. Phys. Lett.},
Unique-ID = {ISI:A1992JB82000009},
}

