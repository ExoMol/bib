ISSN = {1089-5639},
Times-Cited = {2},
Journal-ISO = {J. Phys. Chem. A},
Unique-ID = {ISI:000232612000012},
}

@article{ 03ZhLixx.AlO,
Author = {Zhang, SD and Li, HY},
Title = {Formation mechanism and emission spectrum of AlO radicals in reaction of
   laser-ablated Al Atom and oxygen},
Journal = {CHEMICAL RESEARCH IN CHINESE UNIVERSITIES},
Year = {2003},
Volume = {19},
Number = {3},
Pages = {320-323},
Month = {JUL},
Abstract = {The emission spectrum of AlO radicals was analyzed in 440-540 nm in the
   reaction of laser ablated At beam and O-2. The carrier of spectrum was
   assigned to Deltanu = 0, +/-1, +/-2 vibrational sequences of
   B(2)Sigma(+)-X(2)Sigma(+) transition of AlO radicals, the observed
   maximum vibrational quantum number was root = 6. The rotational and
   vibrational temperatures of B state were estimated at 3000 and 7500 K by
   spectrally simulating the rovibronic population distribution. There is a
   strong evidence that the production of excited Al(S-2) atoms is
   essential to the formation of excited AlO radicals.},
