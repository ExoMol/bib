@article{92CaMcGa.AlO,
Author = {Campbell, M. L. and McClean, R. E. and Garland, N. L. and Nelson, H. H.},
Title = {{Electronic quenching of the B2-SIGMA+ state of ALO}},
Journal = CPL,
Year = {1992},
Volume = {194},
Pages = {187-190},
Abstract = {Electronic quenching rate constants for the B2-SIGMA+ state of AlO were
   determined using a pulsed-laser photolysis laser-induced fluorescence
   technique by examining fluorescence decay rates in the presence of seven
   atomic and molecular collision partners. The two polar molecules CO and
   NO were found to have rate constants of 4.8 x 10(-12) and 1.8 x 10(-10)
   cm3 s-1, respectively. The nonpolar gases He, H2, N2, CO2 and Xe were
   found to be poor quenchers with a bimolecular quenching rate constant
   having an upper bound of 1 X 10(-13) cm3 s-1 for He, H-2, N2 and Xe and
   1 x 10(-12) cm3 for CO2.},
DOI = {10.1016/0009-2614(92)85532-F}
}


@article{92RiCaPa.AlO,
Author = {Rivassilva, J. F. and Campoy, G. and Palma, A.},
Title = {{Improving an algebraic approach to calculate approximate Franck–Condon
factors of diatomic molecules}},
Journal = IJQC,
Year = {1992},
Volume = {43},
Pages = {747-753},
Abstract = {Using the BCH theorem, we express the Hamiltonian of a Morse oscillator
   as a complete series of powers of the creation and annihilation
   operators for the harmonic oscillator. In this way, we improve the
   results of a previous work that uses a Bogoliubov-Tyablikov
   tranformation to calculate the Franck-Condon factors by means of
   equivalent harmonic oscillators potentials.},
DOI = {10.1002/qua.560430603}
}


@article{91SaSaAs.AlO,
