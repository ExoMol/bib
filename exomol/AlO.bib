author = {Banerjee, D. P. K. and Varricatt, W. P. and Ashok, N. M. and Launila, O.},
title= {{Remarkable changes in the near-infrared spectrum of the Nova-like Variable V4332 Sagittari}},
journal= ApJ,
year= {2003},
volume= {598},
