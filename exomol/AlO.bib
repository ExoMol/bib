@article{95BeMoGo.AlO,
Author = {Bescos, B. and Morley, G. and Urena, A. G.},
Title = {Chemiluminescence and reactivity of aluminum monoxide (AlO) in the excited B~$^2\Sigma^+$ state},
Journal = CPL,
Year = {1995},
Volume = {244},
Pages = {407-413},
Abstract = {Using laser ablation of aluminum samples under moderate O-2 pressures
   the chemiluminescence and reactivity of the excited AlO(B((2) Sigma(+)))
   state have been measured, The analysis of the AlO(B) fluorescence
   corresponding to emission from different internal states with various
   oxygen concentrations has allowed us to obtain rate constants values for
   the AlO(B) + O-2 reaction for different vibrational states, The results
   are k((upsilon=0)) = 1.1 x 10(-11) molecule(-1) cm(3) s(-1) and
   k((upsilon=1)) = 1.1 x 10(-11) molecule(-1) cm(3) s(-1).},
DOI = {10.1016/0009-2614(95)00949-5}
}

@article{95SaItKu.AlO,
Author = {Sato, N. and Ito, H. and Kuchitsu, K.},
Title = {{Electronic transition moment of the AlO(B~$^2\Sigma^+$--X~$^2\Sigma^+$) emission. Analysis of the R dependence of the Al$^{2+}$O$^{2-}$ character in the X~$^2\Sigma^+$ state}},
Journal = CPL,
Year = {1995},
Volume = {240},
Pages = {10-16},
Abstract = {The dependence of the electronic transition moment, R(c)(R), of the
   AlO(B (2) Sigma(+)-X (2) Sigma(+)) system on the internuclear distance,
   R, has been determined by an analysis of the intensity distribution of
   the emission spectra as R(e)(R)={[}1 - 2.29(13)(R/Angstrom - 1.5)(2)]
   (1.510 Angstrom less than or equal to R less than or equal to 1.836
   Angstrom). The R dependences of R(e)(R) and the X (2) Sigma(+) similar
   to A (IIi)-I-2 off-diagonal spin-orbit interaction matrix element,
   H-el(SO)(R), have been analyzed simultaneously to evaluate the R
   dependence of the Al2+O2- character in the X (2) Sigma(+) state and that
   of the orbital exponent of the oxygen atom. The results agree with the
   theoretical prediction by Lengsfield and Liu (J. Chem. Phys. 77 (1982)
   6083) that the charge-transfer process, Al2+O2- --> Al+O-, takes place
   in the range of 1.6 Angstrom less than or similar to R < less than or
   similar to 2.1 Angstrom.},
DOI = {10.1016/0009-2614(95)00495-P}
}

