@article{13NaEnxx.C2,
Author = {Nakajima, Masakazu and Endo, Yasuki},
Title = {{Spectroscopic observation of higher vibrational levels of {C}$_2$ through
   visible band systems}},
Journal = JCP,
Year = {2013},
Volume = {139},
pages = {244310},
Abstract = {Higher vibrational levels of the C$_2$ molecule than those observed so far
   were investigated for the X-1 Sigma(+)(g), A(1)Pi(u), a(3)Pi(u),
   c(3)Sigma(+)(u), and d(3)Pi(g) states through the Phillips, Swan, and
   d(3)Pi(g) - c(3)Sigma(+)(u) band systems under a jet-cooled condition.
   The term values and the molecular constants for 21 new vibronic levels
   were determined from rotationally resolved excitation spectra. The
   determined term values and rotational constants were compared to those
   derived from high-level ab initio potential curves. Perturbations
   identified in low J levels of the d(3)Pi(g) (v = 8) state are most
   likely to be caused by the 1(53)Pi(g) (v = 3) state.},
