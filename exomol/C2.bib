@article{88UrBaJa.C2,
author = {R. S. Urdahl and Yihan Bao and William M. Jackson},
title = {{Observation of the {LIF} spectra of {C}$_2$(a 3Πu) and {C}$_2$(A 1Πu)
from the photolysis of {C}$_2${H}$_2$ at 193 nm}},
journal = CPL,
volume = {152},
pages = {485 - 490},
year = {1988},
abstract = {The laser-induced fluorescence (LIF) spectra of the C2(d 3Πg)←C2(a 3Πu),
Swan and C2(C 1Πg)←C2(A 1Πu), Deslandresd'Azambuja systems produced in the multiphoton
photodissociation of \{C2H2\} have been observed. Preliminary analysis of the spectra
shows that the nascent fragments are rotationally excited with J ⩽ 43 and J ⩽ 48
