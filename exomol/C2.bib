title = {{High-resolution spectroscopy of V854 Cen in decline — absorption and emission lines of C2 molecules}},
journal = MNRAS,
year = {2000},
volume = {313},
pages = {L33-L37},
doi = {10.1046/j.1365-8711.2000.03397.x},
URL = {http://dx.doi.org/10.1046/j.1365-8711.2000.03397.x}
