@article{ 25LeRoFe.C2,
Author = {Le Bourlot, J. and Roueff, E. and Federman, S. R. and Ritchey, A. M. and
   Lambert, D. L.},
Title = {{Rotational equilibrium of C2 in diffuse interstellar clouds
   II. Formation-excitation at work?}},
Journal = AA,
Year = {2025},
Volume = {699},
Abstract = {Context. Recent spectroscopic measurements have revealed absorption from
   higher rotational levels in C-2 than previous observations. These
   improvements are accompanied by the availability of updated radiative
   and collisional data. Aims. We revisit the density and radiation field
   intensity diagnostics provided by the observations of many rotational
   levels of interstellar C-2 and extensive molecular information. Methods.
   We built an excitation model of C-2 without spatial structure, including
   levels up to J = 34 where updated radiative and collisional excitation
   data are introduced as well as excitation by chemical formation.
   Results. We confirm the importance of the recent collisional excitation
   rate coefficients of C-2 by molecular H-2. We show that the new higher
   level observations cannot be explained by the standard balance between
   collisional excitation and radiative transitions. We propose that
   chemical excitation at formation provides a plausible mechanism to
   explain the observed high excitation ofC(2). In addition, it allows us
   to lift the degeneracy of the density over radiation field strength
   parameter in the excitation model. Conclusions. A 0D model remains
   limited and it is highly desirable to use a full Photon Dominated Region
   (PDR) model, which includes all excitation processes introduced here and
   full chemical and thermal balance.},
DOI = {10.1051/0004-6361/202554760},
pages = {A217}}

@inproceedings{ 24GeFi.C2,
Author = {Geipel, Clayton M. and Fisher, Brian T.},
Book-Group-Author = {AIAA},
Title = {{Three-Color Pyrometry and C$_2$ Emission Spectroscopy of HTPB
   Combustion}},
Booktitle = {AIAA SCITECH 2024 FORUM},
Year = {2024},
Organization = {AIAA},
doi={10.2514/6.2024-1642},
Abstract = {Combustion temperatures were measured for hydroxyl-terminated
   polybutadiene (HTPB) fuel strands using three-color pyrometry.
   Cylindrical strands were burned in an opposed oxidizer flow.
   Two-dimensional temperature maps from pyrometry indicated that flame
   temperature increased with radial distance from the center of the
   strand. Pyrometry results were generally in agreement with
   previously-reported broadband emission spectroscopy and with a
   one-dimensional computational model of HTPB combustion.
   High-resolution emission spectra were also collected for HTPB flames in
   a band around 480 nm that provides access to the C-2 Swan bands. The
   ExoMol spectral database was used to produce a library of calculated
   spectra. Comparison between each experimental spectrum and the library
   provided a best-fit temperature for the spectrum. Experimental and
   calculated spectra agree very well but indicate a much higher
   temperature than expected from other methods, suggesting that the C-2
   radical is not in thermal equilibrium with the flow. Further
   investigation of the validity of this technique and the significance of
   its results is warranted.},
Publisher = {AMER INST AERONAUTICS \& ASTRONAUTICS},
Address = {1801 ALEXANDER BELL DR, STE 500, RESTON, VA 20191-4344 USA}}

@article{ 24MaLiJi.C2,
Author = {Ma, Liying and Li, Di and Jiang, Pan and Cheng, Min and Gao, Hong},
Title = {Strongly Rotationally Dependent Lifetimes of C<sub>2</sub> in the
   2<SUP>3</SUP>Σ<sub>g</sub> <SUP>-</SUP> State: Implications for
   Predissociation in the Vacuum Ultraviolet Region},
Journal = JPCL,
Year = {2024},
Abstract = {The radiative and photodissociative properties of the dicarbon molecule,
   C-2, in high-lying electronic states are of utmost importance for
   modeling the photochemical processes that occur in various astronomical
   environments. Despite extensive spectroscopic studies in the last two
   centuries, the photodissociation properties of C-2 are still largely
   unknown, particularly for quantum states in the vacuum ultraviolet (VUV)
   region. Here, the lifetimes of C-2 for each individual rovibrational
   level in the recently identified 2(3)Sigma(-)(g) state are measured for
   the first time using a VUV-pump-UV-probe photoionization scheme. The
   lifetimes are found to be strongly dependent on the rotational and
   vibrational quantum levels in the 2(3)Sigma(-)(g) state. The strongly
   rotationally dependent lifetimes observed here indicate that the
   2(3)Sigma(-)(g) state may mainly undergo a predissociation process
   through couplings with nearby repulsive electronic states. The current
   observation could have important applications in modeling the
   interstellar medium and cometary comae.},
DOI = {10.1021/acs.jpclett.4c01605}}

@article{23HaVeWa.C2,
doi = {10.3847/1538-4357/acebc0},
year = {2023},
volume = {955},
pages = {17},
author = {Christian R. Hayes and Kim A. Venn and Fletcher Waller and Jaclyn Jensen and Alan W. McConnachie and John Pazder and Federico Sestito and André Anthony and Gabriella Baker and John Bassett and Joao Bento and Trystyn Berg and Gregory Burley and Jurek Brzeski and Scott Case and Edward Chapin and Timothy Chin and Eric Chisholm and Vladimir Churilov and Adam Densmore and Ruben Diaz and Jennifer Dunn and Michael Edgar and Tony Farrell and Veronica Firpo and Joeleff Fitzsimmons and Juan Font-Serra and Javier Fuentes and Colin Ganton and Manuel Gomez-Jimenez and Tim Hardy and David Henderson and Alexis Hill and Brian Hoff and Michael Ireland and Venu Kalari and Neal Kelly and Urs Klauser and Yuriy Kondrat and Kathleen Labrie and Sam Lambert and Lance Luvaul and Jon Lawrence and Jordan Lothrop and G. Scott Macdonald and Slavko Mali and Steve Margheim and Richard McDermid and Helen McGregor and Bryan Miller and Felipe Miranda and Rolf Muller and Jon Nielsen and Ryan Norbury and Oliver Oberdorf and Naveen Pai and Gabriel Perez and Pablo Prado and Ian Price and Carlos Quiroz and Vladimir Reshetov and Gordon Robertson and Roque Ruiz-Carmona and Ricardo Salinas and Kim M. Sebo and Andrew Sheinis and Matthew Shetrone and Keith Shortridge and Katherine Silversides and Karleyne Silva and Chris Simpson and Greg Smith and Kei Szeto and Julia Tims and Eduardo Toro and Cristian Urrutia and Sudharshan Venkatesan and Lewis Waller and Ivan Wevers and Ramunas Wierzbicki and Marc White and Peter Young and Ross Zhelem},
title = {GHOST Commissioning Science Results: Identifying a New Chemically Peculiar Star in Reticulum II},
journal = {The Astrophysical Journal},
abstract = {{The Gemini High-resolution Optical SpecTrograph (GHOST) is the newest high-resolution spectrograph to be developed for a large-aperture telescope, recently deployed and commissioned at the Gemini-South telescope. In this paper, we present the first science results from the GHOST spectrograph taking during its commissioning runs. We have observed the bright metal-poor benchmark star HD 122563, along with two stars in the ultrafaint dwarf galaxy Reticulum II (Ret ii), one of which was previously identified as a candidate member, but did not have a previous detailed chemical abundance analysis. We find that this candidate (GDR3 0928) to be a bona fide member of Ret ii, and from a spectral synthesis analysis it is also revealed to be a CEMP-r star, with significant enhancements in several light elements (C, N, O, Na, Mg, and Si), in addition to featuring an r-process enhancement like many other Ret ii stars. The light-element enhancements in this star resemble the abundance patterns seen in the CEMP-no stars of other ultrafaint dwarf galaxies, and are thought to have been produced by an independent source from the r-process. These unusual abundance patterns are thought to be produced by faint supernovae, which may be produced by some of the earliest generations of stars.}
}}


