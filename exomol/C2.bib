@ARTICLE{89FeHuxx.C2,
author = {Federman, S. R. and Huntress, Jr., W. T.},
title = {{Diffuse interstellar clouds as a chemical laboratory: The chemistry of diatomic carbon species}},
journal = ApJ,
year = {1989},
volume = {338},
pages = {140-146},
abstract = {The chemistry of C2, CH, and CO in diffuse interstellar clouds is
