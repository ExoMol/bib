@article{51KeRoSc.C2H2,
author = {R. L. Kelly and R. Rollefson and B. S. Schurin},
title = {{The infrared dispersion of acetylene and the dipole moment of the {C-H} bond}},
journal = JCP,
year = {1951},
volume = {19},
number = {12},
pages = {1595-1599},
abstract = {The index of refraction of acetylene has been measured at 55 wavelengths
in the infrared between 1 and 20 microns. The contributions of the molecular vibrations
to the index of refraction have been determined and interpreted in terms of effective
charges on the constituent atoms. According to the analysis, during the infrared‐active
bending vibration the hydrogen atoms have an effective charge of 0.991×10−10 esu.
The product of this effective charge by the separation of the carbon and hydrogen
atoms is interpreted as giving an estimate of the C–H bond moment, the value obtained
being (1.05±0.01)×10−18 esu cm. For the active stretching vibration, the computed
effective charge was 0.515×10−10 esu, which is the rate of change of bond moment
with internuclear distance. A comparison of these results with previous work indicates
a possible regularity in the C–H bond moment in going from methane to ethylene
to acetylene, although the regularity may be only apparent.},
doi = {10.1063/1.1748128}
}


