@article{72Pliva.C2H2,
author = {Josef Pl\'iva},
title = {{Spectrum of acetylene in the 5-micron region}},
Journal = JMS,
year = {1972},
volume = {44},
pages = {145-164},
abstract = {Three Πu-Σg+ bands, 2ν4 + ν5 at 1959.697 and 1940.003 cm−1 and 3ν5
at 2169.166 cm−1, accompanied by fifteen Σ+-Π, Σ−-Π, Δ-Π, and Π-Π “hot” bands
occurring in the region between 1830 and 2250 cm−1 were measured and analyzed.
The Σu+-Σg+ band 3ν4 + ν5 at 2560.600 cm−1 was also measured. The upper states
