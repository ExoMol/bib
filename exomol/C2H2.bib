@article{94WeBlHa.C2H2,
author = {M. Weber and W. E. Blass and G. W. Halsey and J. J. Hillman},
title = {{I-Resonance perturbation of IR intensities in $^{12}$C$_2$H$_2$ near 13.7 $\mu$m}},
Journal = JMS,
year = {1994},
volume = {165},
pages = {107-123},
abstract = {Using TDL and FT spectrometric data at 0.0005 and 0.0025 cm−1 spectral
resolution, respectively, individual line intensities of the ν5 fundamental and
the seven strongest hot bands, ν4 + ν5 ← ν4 and 2ν5 ← ν5, of $^{12}$C$_2$H$_2$
in the 13.7-μm region have been measured. Except for the 2ν2f5 ← ν1f5 hot band,
strong intensity perturbation due to l-type doubling and resonances have been
quantitatively determined for all hot bands. The zeroth-order vibrational dipole
moment derivative for the ν5 bending mode is identical within 10$\%$ for all bands
observed. Since an extra factor of 12 appears in the square vibrational transition
moment for the combination band, its intensity is reduced by half and is comparable
in magnitude to the 2ν5 ← ν5 hot bands. For the ν5 fundamental a square dipole moment
derivative of Γ2v = 0.09069(24)D2 has been determined.},
doi = {10.1006/jmsp.1994.1114}
}


