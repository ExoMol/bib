@article{05EdBaMa.C2H2,
Author = {Edwards, C S and Barwood, G P and Margolis, H S and Gill, P and Rowley, W R C},
Title = {{High-precision frequency measurements of the $v_1 + v_3$ combination
   band of $^{12}$C$_2$H$_2$ in the 1.5 $\mu$m region}},
Journal = JMS,
Year = {2005},
Volume = {234},
Pages = {143-148},
Abstract = {A pair of 1.5 $\mu$m semiconductor laser frequency standards have been
   developed for optical telecommunications use, stabilised to transitions
   of $^{12}$C$_2$H$_2$, and (C2H2)-C-13, using cavity-enhanced Doppler-free
   saturation absorption spectroscopy. The absolute frequencies of 41 lines
   of the v(1) + v(3) band of $^{12}$C$_2$H$_2$, covering the spectral region
   1520-1545 nm, have been measured by use of a passive optical
   frequency,comb generator, referenced to (C2H2)-C-13 transitions of known
   frequency. The mean experimental uncertainties (coverage factor k = 1)
   of the frequency values are 3.0 kHz (type A) and 10 kHz (type B).
   Improved values of the band origin v(0), rotational constants B' and
   B{''}, and centrifugal distortion coefficients D', D{''}, H', and H{''}
   are presented.},
DOI = {10.1016/j.jms.2005.08.014},
Keywords = {molecular spectroscopy; laser frequency standards; optical
   telecommunications; diode laser spectroscopy; optical frequency
   metrology},
Keywords-Plus = {ACETYLENE-STABILIZED LASER; 1.5 MU-M; ABSORPTION-LINES; 1542 NM;
