@article{07RoHeAu.C2H2,
Author = {Robert, S. and Herman, M. and Auwera, J. Vander and Di Lonardo, G. and
   Fusina, L. and Blanquet, G. and Lepere, M. and Fayt, A.},
Title = {{The bending vibrations in $^{12}$C$_2$H$_2$: global vibration-rotation
   analysis}},
Journal = MP,
Year = {2007},
Volume = {105},
Pages = {559-568},
Abstract = {A total of 3155 vibration-rotation line positions providing accurate
   information on all known vibration-rotation states up to 3000 cm$^{-1}$ in
   $^{12}$C$_2$H$_2$ were gathered from the literature. An additional 966 lines
   were assigned on new spectra. The full data set was simultaneously
   fitted using an appropriate Hamiltonian, with 85 refined parameters. A
   dimensionless standard deviation equal to 0.86, accounting for 92\% of
   the lines, was achieved. The vibrational transition dipole moment of the
   v(5) = 1 <- v(4) = 1 band observed in the far-infrared was estimated to
   be vertical bar mu(5 <- 4)vertical bar = 0.55 D with 20\% accuracy, from
   absolute intensity measurements for a few Q-branch lines.},
DOI = {10.1080/00268970601099261},
Keywords-Plus = {DIODE-LASER MEASUREMENTS; 13.7 MU-M; BROADENING COEFFICIENTS;
   LOW-TEMPERATURES; NU(5) BAND; INFRARED-SPECTRUM; RAMAN-SPECTRA; Q
   BRANCHES; MOLECULAR-SPECTROSCOPY; INTENSITY DISTRIBUTION},
}


@article{07TrMaDa.C2H2,
Author = {Tran, H. and Mandin, J. -Y. and Dana, V. and Regalia-Jarlot, L. and
   Thomas, X. and Von der Heyden, P.},
Title = {{Line intensities in the 1.5-$\mu$m spectral region of acetylene}},
Journal = JQSRT,
Year = {2007},
Volume = {108},
Pages = {342-362},
Abstract = {Line intensities are measured for 546 transitions belonging to 13 bands
   of the main isotopologue $^{12}$C$_2$H$_2$ of the acetylene molecule, in the
   1.5-$\mu$m spectral domain. A multispectrum fitting procedure is used to
   retrieve line parameters from Fourier transform spectra. Prior to this
   work, line intensities were known for only 4 bands in this spectral
   region, from the work of El Hachtouki and Vander Auwera {[}Absolute line
   intensities in acetylene: the 1.5 $\mu$m region. J Mol Spectrosc
   2002:216:355-62]. An excellent agreement is found with the results of
   these authors, showing that the accuracy of both results is likely
   better than 1\% for the strong bands. However, the spectrum becomes very
   crowded when one wants to study weaker bands, so that the average
   accuracy of the intensities reported in the present work is 5\%. From
   these data, vibrational transition dipole moments squared and
   Herman-Wallis coefficients have been determined for all the bands.},
DOI = {10.1016/j.jqsrt.2007.04.008},
Keywords = {acetylene; infrared spectroscopy; vibro-rotational transitions; line
   intensities; transition dipole moment},
Keywords-Plus = {INTERNAL PARTITION SUMS; PRESSURE-INDUCED SHIFT; ENERGY-LEVELS;
