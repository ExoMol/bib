@article{96BrToWe.C2H2,
Author = {Brooke, T. Y. and Tokunaga, A. T. and Weaver, H. A. and Crovisier, J and
BockeleeMorvan, D and Crisp, D},
Title = {{Detection of acetylene in the infrared spectrum of comet Hyakutake}},
Journal = {Nature},
Year = {1996},
Volume = {383},
Pages = {606-608},
Abstract = {Comets are rich in volatile materials, of which roughly 80\% (by number)
are mater molecules(1). Considerable progress(2-4) being made in
identifying the other volatile species, the abundances of which should
enable us to determine whether comets formed primarily from ice-covered
interstellar grains(5), or from material that was chemically processed
in the early solar nebula(6,7). Here we report the detection of
acetylene (C2H2,) in the infrared spectrum of comet C/1996 B2
(Hyakutake). The estimated abundance is 0.3-0.9\%, relative to water,
which is comparable to the predicted solid-phase abundance in cold
interstellar clouds. This suggests that the volatiles in comet Hyakotake
may have come from ice-covered interstellar grains, rather than material
processed in the accretion disk out of which the Solar System formed.},
DOI = {10.1038/383606a0},
Keywords-Plus = {Chemical-Models; Molecules; Fluorescence; Atmospheres; Resolution;
Halley; Gas},
}


@article{96KeMeKl.C2H2,
Author = {Keppler, K A and Mellau, G C and Klee, S and Winnewisser, B P and
   Winnewisser, M and Pliva, J and Rao, K N},
Title = {{Precision measurements of acetylene spectra at 1.4-1.7 $\mu$m recorded
   with 352.5-m pathlength}},
Journal = JMS,
Year = {1996},
Volume = {175},
Pages = {411-420},
Abstract = {The spectrum of acetylene (C2H2) in the 1.5 $\mu$m region has been
   recorded at room temperature with Doppler-broadening-limited resolution
   at pathlengths up to 352.5 m. The spectra were obtained by using an all
   vacuum Fourier transform infrared (FTIR) spectrometer with a multipass
   cell of 4 m base length attached to it. The calibration of the data is
   discussed in detail. Thirty-three subbands have been detected in this
   study for C2H2 and five for (CCH2)-C-12-C-13. These include ten new
   bands and nine new states in the parent species, and two new bands and
   two new states in the (CCH2)-C-12-C-13 species. Updated energy level
   diagrams for HCCH and (HCCH)-C-13 are shown. The spectroscopic constants
   obtained from the HCCH spectrum are reported.},
DOI = {10.1006/jmsp.1996.0047},
Keywords-Plus = {INFRARED-SPECTRA; STATES; CONSTANTS; C-12(2)H2; BANDS; C2H2; C-13},
}


@article{96NaLaAw.C2H2,
Author = {Nakagawa, K and deLabachelerie, M and Awaji, Y and Kourogi, M},
Title = {{Accurate optical frequency atlas of the 1.5-$\mu$m bands of acetylene}},
Journal = JOSAB,
Year = {1996},
Volume = {13},
Pages = {2708-2714},
Abstract = {We have measured the absolute optical frequencies of 90 rovibrational
   lines of the $^{12}$C$_2$H$_2$ and (C2H2)-C-13 1.5-$\mu$m bands. Using an
   optical frequency comb generator, we could precisely measure the
   relative frequencies between two lasers stabilized on the saturation dip
   of the rovibrational lines up to 2 THz apart. The absolute frequencies
   were determined with respect to the Rb two-photon transition at 0.778 mu
   m with a HCN rovibration line at 1.556 $\mu$m used as an intermediate
   reference. This measurement provides an accurate frequency atlas of
   acetylene 1.5-$\mu$m bands with an uncertainty of similar to 10(-9).},
DOI = {10.1364/JOSAB.13.002708},
Keywords-Plus = {1.5 MU-M; ABSORPTION-LINES; DIODE-LASER; DIFFERENCE MEASUREMENT;
