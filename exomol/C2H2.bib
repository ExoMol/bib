DOI = {10.1063/1.1531651},
Keywords = {acetylene; $^{12}$C$_2$H$_2$, and (C2D2)-C-12, and (C2H2)-C-13; energy of the
   vibrational states; geometrical structure; partition functions;
   principal rotational constants of the vibrational states; vibrational
   frequencies and anharmonicities; vibration-rotation interaction
   parameters; vibrational level density},
Keywords-Plus = {DIODE-LASER SPECTROSCOPY; DISPERSED FLUORESCENCE-SPECTRUM; RESOLUTION
   RAMAN-SPECTROSCOPY; SAPPHIRE RING LASER; MU-M REGION;
   ABSORPTION-SPECTRUM; INFRARED-SPECTRUM; ROTATION SPECTRA; ENERGY
   PATTERN; C-13-CONTAINING ACETYLENE},
}


@article{03HeKeHu.C2H2,
Author = {Herregodts, F and Kerrinckx, E and Huet, T R and Vander Auwera, J},
Title = {{Absolute line intensities in the $\nu_1+3\nu_3$ band of $^{12}$C$_2$H$_2$ by
   laser photoacoustic spectroscopy and Fourier transform spectroscopy}},
Journal = MP,
Year = {2003},
Volume = {101},
Pages = {3427-3438},
Abstract = {Line intensities and self-broadening coefficients in the nu(1) + 3(nu3)
   band of $^{12}$C$_2$H$_2$ near 0.8 mum at room temperature were measured by
   means of both laser photoacoustic and Fourier transform spectroscopy. An
   experimental protocol has been developed to obtain absolute intensities
   from the photoacoustic measurements. Namely, the spectrometer was
   calibrated using water vapour line intensities available in Hitran 1996
   {[}L. S. Rothman et al . (1998) J. quant. Spectrosc. Radiat. Transfer ,
   60, 665-710]. These photoacoustic line intensities were found to be on
   average 5\% higher than corresponding measurements performed using
   Fourier transform spectroscopy, the accuracy of the latter being
   estimated to better than 4\%. The accuracy of the photoacoustic
   intensities is discussed. Previous results from the literature {[}F.
   Herregodts, D. Hurtmans, J. Vander Auwera, and M. Herman (1999) J. chem.
   Phys. , 111, 7954-7960] are revised.},
DOI = {10.1080/00268970310001632426},
Keywords-Plus = {SAPPHIRE RING LASER; HIGH-RESOLUTION; ABSORPTION-BAND; ACETYLENE;
   CM(-1); REGION; HCN; COEFFICIENTS; TRANSITIONS; DATABASE},
}


@article{03JaMaDab.C2H2,
Author = {Jacquemart, D and Mandin, J Y and Dana, V and Regalia-Jarlot, L and
   Plateaux, J and Decatoire, D and Rothman, L S},
Title = {{The spectrum of acetylene in the 5-$\mu$m region from new line-parameter
   measurements}},
Journal = JQSRT,
Year = {2003},
Volume = {76},
Pages = {237-267},
Abstract = {Using Fourier Transform spectra (unapodized FWHM: 0.002 cm$^{-1}$) of
   acetylene $^{12}$C$_2$H$_2$, absolute line positions have been obtained in the
   5-mum region for about 490 lines belonging to 15 hot bands arising from
   the nu(4) = I or nu(5) = I lower vibrational level, improving the data
   available in this spectral region. Absolute line intensities have been
   measured for about 410 lines, from which vibrational transition dipole
   moments squared could be determined, as well as empirical Herman-Wallis
   coefficients when it was possible. The self-broadening coefficients of
   more than 370 lines have been measured at room temperature, showing that
   no vibrational dependence exists for the bands of acetylene already
   studied, and allowing a significant determination of their rotational
   dependence. Furthermore, about 120 measured values of self-shifting
   coefficients are tentatively proposed. Combining results published for
   other bands, and using the smoothed line-parameter values obtained in
   this work, a HITRAN-format line list of the acetylene molecule has been
   set up in this spectral region. (C) 2002 Elsevier Science Ltd. All
   rights reserved.},
DOI = {10.1016/S0022-4073(02)00055-9},
Keywords = {acetylene; infrared; vibro-rotational transitions; Fourier transform
   spectroscopy; transition dipole moment; line parameters},
Keywords-Plus = {LOW-TEMPERATURE; NU(5) BAND; C2H2; INTENSITIES},
