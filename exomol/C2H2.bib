title = {{Investigations of several infrared bands of $^{12}$C$_{2}$H $_{2}$
and studies of the effects of vibrational rotational interactions}},
Journal = JMS,
year = {1972},
volume = {44},
pages = {131-144},
abstract = {Twenty seven infrared bands of acetylene (12C2H2) in seven selected
