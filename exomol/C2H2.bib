
@article{19NuAlCh.C2H2,
author = {Jacob N\"{u}rnberg and Cesare G. E. Alfieri and Zaijun Chen and Dominik Waldburger and Nathalie Picqu\'{e} and Ursula Keller},
journal = {Opt. Express},
keywords = {Fiber lasers; Laser beams; Quantum cascade semiconductor lasers; Semiconductor lasers; Solid state lasers; Tunable lasers},
pages = {3190--3199},
title = {An unstabilized femtosecond semiconductor laser for dual-comb spectroscopy of acetylene},
volume = {27},
year = {2019},
doi = {10.1364/OE.27.003190},
abstract = {Dual-comb systems based on two optical frequency combs of slightly different line spacing emerge as powerful tools in spectroscopy and interferometry. Semiconductor lasers have a high impact in continuous-wave tunable laser spectroscopy. Here we demonstrate the first dual-comb interferometer based on a single femtosecond semiconductor laser: a dual-comb modelocked optically pumped external-cavity surface-emitting laser (MIXSEL) ideally suited for a 1 to 10 GHz comb spacing. At a center wavelength of 1.03 \&\#x00B5;m (9709 cm\&\#x2212;1) we measured acetylene gas transmittance with a resolution of 2.7 GHz in 100 ms with residual errors of less than 3\% using thousand comb lines without aliasing effects.},
}

@article{ 18TwHaSe.C2H2,
Author = {Twagirayezu, Sylvestre and Hall, Gregory E. and Sears, Trevor J.},
Title = {{Frequency measurements and self-broadening of sub-Doppler transitions in
   the nu(1) + nu(3) band of C2H2}},
Journal = JCP,
Year = {{2018}},
Volume = {{149}},
Abstract = {{Frequency comb-referenced measurements of sub-Doppler laser saturation
   dip absorption lines in the nu(1) + nu(3) band of acetylene near 1.5 mu
   m are reported. These measurements include transitions involving higher
   rotational levels than previously frequency measured in this band. The
   accuracy of the measured frequencies is typically better than 10 kHz.
   Measurements of the observed sub-Doppler line widths as a function of
   pressure showed that the self-pressure-broadening coefficients are about
   3.5 times larger than those derived from conventional pressure
   broadening of unsaturated Doppler-limited spectra. This is attributed to
   the contribution of velocity-changing collisions to the total dephasing
   rate in the low pressure sub-Doppler measurements. At higher pressures,
   when the homogeneous broadening becomes comparable to the typical
   Doppler shift per elastic collision, the velocity changing collisions
   cease to contribute significantly to the incremental pressure
   broadening. A time-dependent soft collision model is developed to
   illustrate the transition between low and high pressure regimes of
   sub-Doppler pressure-broadening.}},
DOI = {{10.1063/1.5047410}},
pages= {154308}}

@article{00MaDaCl.C2H2,
Author = {Mandin, J Y and Dana, V and Claveau, C},
Title = {{Line intensities in the $\nu_5$ band of acetylene $^{12}$C$_2$H$_2$}},
Journal = JQSRT,
Year = {2000},
Volume = {67},
Pages = {429-446},
Abstract = {Using FTS spectra (apodized FWHM: 0.0014 cm$^{-1}$), absolute intensities
   have been measured for about 80 lines of the nu(5) band of $^{12}$C$_2$H$_2$
   at 13.6 $\mu$m, with an accuracy of +/- 5\%, improving the already
   published measured values. The vibrational transition dipole moment
   squared has been determined: {[}R-0](2) = (9.81 +/- 0.11) x 10(-2) D-2
   (1D = 3.33546 x 10(-30) C m), as also the Herman-Wallis coefficients:
   A(1)(RP) = (-9.34 +/- 1.85) x 10(-4), and A(2)(RP) = (4.30 +/- 0.89) x
   10(-5). On the whole, the measured line intensities have been found
   greater than the 1996 HITRAN values by about 14\%.},
DOI = {10.1016/S0022-4073(00)00010-8},
Keywords = {acetylene; infrared; vibro-rotational transitions; Fourier transform
   spectroscopy; measurement of absolute line intensities; transition
   dipole moment; band intensity},
Keywords-Plus = {DIODE-LASER SPECTROSCOPY; INTEGRATED INFRARED INTENSITIES;
   VIBRATIONAL-ENERGY PATTERN; 13.7 MU-M; BROADENING COEFFICIENTS;
   APPROXIMATE CONSTANTS; LOW-TEMPERATURES; C2H2; REGION; SPECTRA},
}


@article{00MoDuJa.C2H2,
Author = {Moss, D B and Duan, Z C and Jacobson, M P and O'Brien, J P and Field, R W},
Title = {{Observation of coriolis coupling between $\nu_2+4\nu_4$ and $7\nu_4$ in
   acetylene $^1\Sigma^+_g$ by stimulated emission pumping spectroscopy}},
Journal = JMS,
Year = {2000},
Volume = {199},
Pages = {265-274},
Abstract = {Stimulated emission pumping (SEP) spectroscopy has been used to examine
   a low energy region (E-vib approximate to 4400 cm$^{-1}$) of (X) over
   tilde(w) Sigma(g)(+) acetylene at higher resolution than was possible in
   previous dispersed fluorescence studies. The expected bright state,
   nu(2) + 4 nu(4) is observed to be coupled to the nearly degenerate 7
   nu(4), state by a Coriolis mechanism. A least-squares analysis yields
   values for zero-order vibrational energies, rotational constants, and a
   Coriolis-coupling coefficient that are all consistent with expectations.
   Calculated relative intensities of SEP transitions, accounting for
   interference due to axis-switching effects, are also consistent with
   observations. Implications of the observed Coriolis resonance with
   regard to global acetylene vibrational dynamics are also discussed.},
DOI = {10.1006/jmsp.1999.7994},
Keywords = {acetylene; Coriolis coupling; IVR},
Keywords-Plus = {INTRAMOLECULAR VIBRATIONAL REDISTRIBUTION; CHANNEL 3 REGION; ENERGY
   REDISTRIBUTION; APPROXIMATE CONSTANTS; EXCITED ACETYLENE; INTERNAL
   ENERGY; RELAXATION; SPECTRA; DYNAMICS; STATE},
}


