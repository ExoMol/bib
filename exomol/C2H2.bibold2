@article{17Caxxxx.C2H2,
author = {Tucker CarringtonJr.},
title = {Perspective: Computing (ro-)vibrational spectra of molecules with more than four atoms},
journal = JCP,
volume = {146},
pages = {120902},
year = {2017},
doi = {10.1063/1.4979117},
    abstract = { In this perspective, I review methods for computing (ro-)vibrational energy levels and wavefunctions of molecules with more than four atoms. I identify three problems one confronts (1) reducing the size of the basis; (2) computing hundreds of eigenvalues and eigenvectors of a large matrix; (3) calculating matrix elements of the potential, and present ideas that mitigate them. Most modern methods use a combination of these ideas. I divide popular methods into groups based on the strategies used to deal with the three problems. }
}

@article{98ChGuLi.C2H2,
author = {Rongqing Chen and Hua Guo and Li Liu and James T. Muckerman},
title = {Symmetry-adapted filter diagonalization: Calculation of the vibrational spectrum of planar acetylene from correlation functions},
journal = JCP,
volume = {109},
pages = {7128-7136},
year = {1998},
doi = {10.1063/1.477396},
abstract = {A symmetry-adapted filter-diagonalization method is used to calculate the vibrational spectrum of planar acetylene. In this method, vibrational eigenvalues in a given symmetry are obtained by solving a generalized eigenproblem in which the Hamiltonian and overlap matrices are assembled from symmetry-adapted correlation functions. Since no filtered state is explicitly needed, the calculation requires a relatively small memory. The numerical efficiency is further improved as the correlation functions belonging to various symmetry species are generated from a single wave packet. Comparison with existing data for the acetylene system confirms its accuracy and efficiency. }
}

@article{92SaKaxx.C2H2,
title = {Diode laser spectroscopy of acetylene: $3\nu_3+\nu_1$3 Region at 0.77 $\mu$m},
journal = JMS,
volume = {154},
pages = {277 - 287},
year = {1992},
doi = {10.1016/0022-2852(92)90208-6},
author = {Jun Sakai and Mikio Katayama},
abstract = {High resolution spectra of acetylene have been measured in the region between 12 900 and 13 080 cm-1 with a GaAs diode laser spectrometer. Five bands (3nu1 + nu3 of $^{12}$C$_2$H$_2$ and 12C13CH2, and 3nu1 + nu3 + nu41 - nu41, 4nu3 + nu51 - nu41, and 3nu1 + nu3 + nu51 - nu51 of $^{12}$C$_2$H$_2$) have been assigned and analyzed yielding new or improved spectroscopic constants for the upper states. Perturbations observed in the 3nu1 + nu3 and 3nu1 + nu3 + nu51 - nu51 bands of $^{12}$C$_2$H$_2$ have been analyzed by taking account of the Fermi interactions.}
}

