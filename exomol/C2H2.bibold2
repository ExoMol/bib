@article{93WeBlNa.C2H2,
author = {Mark Weber and William E Blass and Shachar Nadler and George W Halsey and William C Maguire and John J Hillman},
title = {{Resonance effects in C$_2$H$_2$ near 13.7 $\mu$m. Part H: The two quantum hotbands}},
Journal = SCAA,
volume = {49},
pages = {1659 - 1681},
year = {1993},
doi = {10.1016/0584-8539(93)80124-S},
abstract = {This article is the second part on ‚Ñì-resonance effects on the rotation-vibration bands of acetylene observed in the ŒΩ5 fundamental region. While the first part concentrated on the energy level analysis of the fundamental and the seven strongest hotbands originating in the ŒΩ4 and ŒΩ5 excited states for both major isotopes [Spectrochim. Acta 48A, 1203 (1992)], this article summarizes the results of the analysis of the hotbands 2ŒΩ4 + ŒΩ5 ‚Üê 2ŒΩ4, ŒΩ4 + 2ŒΩ5 ‚Üê ŒΩ4 + ŒΩ5, and 3ŒΩ5 ‚Üê 2ŒΩ5 from which improved molecular constants for the 2ŒΩ4 and three quantum energy levels were derived for the major isotope $^{12}$C$_2$H$_2$. The mixing levels within the excited vibrational states due to vibrational and rotational ‚Ñì-resonance effects are discussed which lead to the identification of the strong ‚Äúforbidden‚Äù Œî‚Ñì 3 band, 2ŒΩ4+ ŒΩ35‚Üê2ŒΩOe4 as a result of ‚Ñì-resonance intensity perturbation.}
}

@article{94WeBlHa.C2H2,
author = {M. Weber and W. E. Blass and G. W. Halsey and J. J. Hillman},
title = {I-Resonance Perturbation of IR Intensities in $^{12}$C$_2$H$_2$ near 13.7 Œºm},
Journal = JMS,
volume = {165},
pages = {107 - 123},
year = {1994},
doi = {10.1006/jmsp.1994.1114},
abstract = {Using TDL and FT spectrometric data at 0.0005 and 0.0025 cm‚àí1 spectral resolution, respectively, individual line intensities of the ŒΩ5 fundamental and the seven strongest hot bands, ŒΩ4 + ŒΩ5 ‚Üê ŒΩ4 and 2ŒΩ5 ‚Üê ŒΩ5, of $^{12}$C$_2$H$_2$ in the 13.7-Œºm region have been measured. Except for the 2ŒΩ2f5 ‚Üê ŒΩ1f5 hot band, strong intensity perturbation due to l-type doubling and resonances have been quantitatively determined for all hot bands. The zeroth-order vibrational dipole moment derivative for the ŒΩ5 bending mode is identical within 10$\%$ for all bands observed. Since an extra factor of 12 appears in the square vibrational transition moment for the combination band, its intensity is reduced by half and is comparable in magnitude to the 2ŒΩ5 ‚Üê ŒΩ5 hot bands. For the ŒΩ5 fundamental a square dipole moment derivative of Œì2v = 0.09069(24)D2 has been determined.}
}

