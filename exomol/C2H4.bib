@ARTICLE{00GoTaOn.C2H4,
  author = {Goh, K L and Tan, T L and Ong, P P and Teo, H H},
  title = {{High-resolution FTIR spectroscopy of the nu(11) and nu(2)+nu(7)
	bands of ethylene-d(4)}},
