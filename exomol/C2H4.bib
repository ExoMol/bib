@ARTICLE{10LeTaxx.C2H4,
  author = {Lebron, G B and Tan, T L},
  title = {{High-resolution FTIR measurement and analysis of the nu(3) band
	of C2H3D}},
  journal = JMS,
  year = {2010},
  volume = {261},
  pages = {119-123},
  abstract = {The Fourier transform infrared (FTIR) spectrum of the 53 band of C2H3D
