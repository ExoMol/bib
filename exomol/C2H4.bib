  title = {{Interaction of vibrational fundamental and combination states of
	ethylene in the 3 mu m region}},
  journal = JMS,
  year = {1997},
  volume = {185},
  pages = {31-47},
  abstract = {High-resolution IR spectra of ethylene cooled in a molecular jet have
