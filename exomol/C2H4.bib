@ARTICLE{13UlGrAs.C2H4,
  author = {Ulenikov, O N and Gromova, O V and Aslapovskaya, Y S and Horneman,
	V.-M.},
  title = {{High resolution spectroscopic study of C2H4: Re-analysis of the
