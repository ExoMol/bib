@article{  95Bernath.C3,
author = {Bernath P. F.},
title = {{Laboratory astrophysics and molecular astronomy of pure carbon molecules}},
journal = {Advances in Space Research},
year = {1995},
volume = {15},
number = {3},
pages = {15-23},
abstract = {The pure carbon molecules Cn are currently of great experimental and
theoretical interest. Our work in this area begins with detection of the SiC molecule,
which is isovalent with C2. New infrared electronic transitions of C2 and C3 were
discovered by emission spectroscopy of hydrocarbon dicharges. The C3 and C5 molecules
were found by infrared vibration-rotation spectroscopy of the prototypical obscured
carbon star, IRC+10216. C7 and C9 were searched for in the same source, but not found.
The laboratory infrared emission spectrum of C60 was recorded to aid in a search
for C60 in extraterrestrial sources.},
doi = {10.1016/S0273-1177(99)80059-3},
url = {http://dx.doi.org/10.1016/S0273-1177(99)80059-3}
}


@article{ 93SzVaxx.C3,
Author = {Szczepanski, J. and Vala, M.},
Title = {{The NU(1)+NU(3) combination-mode of C3 in AR and KR matrices - evidence
   for a bent structure}},
Journal = JCP,
Year = {1993},
Volume = {99},
Number = {10},
Pages = {7371-7375},
Abstract = {The nu1 + nu3 combination mode of laser-ablated C3 has been observed in
   Ar and Kr matrices for all six possible C-12/C-13 isotopomers. The nu1
   totally symmetric mode frequencies have been determined by difference
   with the known nu3 asymmetric stretching frequencies, corrected for
   anharmonicity effects. A comparison of certain isotopomer pairs reveals
   that C3 is bent in these matrices. A normal coordinate analysis using
   four force constants resulted in an excellent fit of the 12 observed
   frequencies (nu1 and nu3 for six isotopomers). From the fit, a value for
   the nu2 bending mode frequency of 82 cm-1 was obtained. This is shown to
   be in good agreement with the value obtained from the C3 phosphorescence
   spectrum. Finally, the molecular bending angle which was determined to
   be 160.0-degrees (in Ar and Kr) is shown to match very well with the
   predicted value from ab initio theory (161.6-degrees) and from a fitting
   of a Morse oscillator-rigid bender potential to gas phase vibrational
   frequencies (162.5-degrees). The influence of the matrix environment on
   the vibrational force constants and frequencies of C3 is discussed.},
Keywords = {Vibration Energy-Levels; Small Carbon Clusters; C-3; Spectroscopy;
   Molecule},
DOI = {10.1063/1.465717},
URL = {http://dx.doi.org/10.1063/1.465717}
}


@article{  94AlJeNo.C3,
author = {Alml\"{o}f, J. and Jensen, P. and Northrup, F. J. and Rohlfing, C. M.
and Rohlfing, E. A. and Sears, T. J.},
title = {{Comment on ‘‘The ν1+ν3 combination mode of C3 in Ar and Kr matrices:
Evidence for a bent structure’’} [J. Chem. Phys. 99, 7371 (1993)]},
