@article{  88MaKaKa.C3,
author = {Matsumura, K. and Kanamori, H. and Kawaguchi, K. and Hirota, E.},
title = {{Infrared diode laser kinetic spectroscopy of the ν3 band of C3}},
journal = JCP,
year = {1988},
