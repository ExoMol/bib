journal = THEOCHEM,
year = {1991},
volume = {234},
pages = {509-514},
abstract = {\{CASSCF\} and \{CI\} calculations have been performed for the ground
and electronically excited states of the linear \{C3\} (D∞h) molecule. Some new
low-lying valence states, in particular quintet states, are described. Tv
and \{T0\} values are presented for the excited states, which are of experimental
interest; an essential improvement over existing theoretical data has been achieved,
especially for the 1Σ+u state.},
