@article{91BaPaDu.CN,
Author = {Bag, M. and Panja, M. M. and Dutt, R. and Varshni, Y. P.},
Title = {{Application of the shifted large-N method to the rovibrational spectroscopy
of diatomic-molecules}},
Journal = JCP,
Year = {1991},
Volume = {95},
Pages = {1139-1143},
Abstract = {Computations of energy levels, expectation values of various powers of
   internuclear separation, and Franck-Condon factors for molecular
   potentials have been carried out within the framework of shifted large-N
   technique.  In contrast to other perturbative as well as semiclassical
   calculations, we obtain compact analytic expressions for these
   quantities.  For illustration, our predicted results for perturbed
   Kratzer potential are compared with those derived by numerical
   integration.  The method gives quite accurate results for a wide range
   of rovibrational quantum numbers.  Further possible applications, in
   particular, to Morse oscillators are finally discussed.},
DOI = {10.1063/1.461143}
}


@article{91NaKoKu.CN,
Author = {Nagata, T. and Kondow, T. and Kuchitsu, K. and Tabayashi, K. and Shobatake, K.},
Title = {{Collision energy-dependence of the cross-sections for production of
   CN(B2-SIGMA+) in the reactions of Ar (3P0,2) and Kr (3P0,2) with BrCN}},
Journal = JCP,
Year = {1991},
Volume = {95},
Pages = {1011-1019},
Abstract = {Cross sections for the formation of CN(B 2-SIGMA+) in collisions of
   Ar(3P0,2) and Kr(3P0,2) with BrCN were measured in a crossed beam
   geometry as a function of the collision energy.  An archeated metastable
   rare-gas beam was velocity selected by a time-of-flight method, and the
   collision energy E(c) was varied in the range of 0.7-5.2 eV for
   Ar(3P0,2) and 1.0-5.2 eV for Kr(3P0,2).  The cross sections were found
   to be proportional to E(c)-m, with m = 0.355 +/- 0.008 for Ar(3P0,2) and
   0.124 +/- 0.010 for Kr(3P0,2).  The relative cross sections were
   normalized against the known absolute cross section for collisional
   excitation transfer from Ar(3P0,2) to N2.  The absolute cross sections
   thus determined were, for example, 16.0 +/- 4.9 angstrom2 for the
   Ar(3P0,2) + BrCN system and 42.3 +/- 14.8 angstrom2 for the Kr(3P0,2) +
   BrCN system at the collision energy of 1.0 eV.  The results were
   explained in terms of the curve-crossing mechanism that an ion-pair
   {[}Rg+BrCN-] intermediate state intervenes between the entrance and the
   exit covalent states.},
DOI = {10.1063/1.461180}
}


