@article{92IwOhBa.CN,
Author = {Iwamoto, E. and Ohlsson, K. E. A. and Baxter, D. C. and Frech, W.},
Title = {{Spatially resolved spectroscopic measurements of CN, C2 and Al in
   graphite tube electrothermal atomizers under various operating-conditions}},
Journal = {J. Anal. At. Spectrom.},
Year = {1992},
Volume = {7},
Pages = {1063-1068},
Abstract = {Direct, spatially resolved, spectroscopic measurements of C2 in
   side-heated (spatially isothermal) and Massmann-type (spatially
   non-isothermal) atomizers showed that the partial pressure of gaseous
   carbon (pC2) is in thermodynamic, heterogeneous equilibrium for
   temperatures above 2970 K. No evidence for over-equilibrium excesses of
   C2 was obtained. The pCN exhibits a gradient, decreasing from the bottom
   to the top of the graphite tube, indicating the importance of air
   ingress through the injection hole. Further, the pCN is reduced by the
   presence of aqueous solutions, primarily close to the point of sample
   deposition, revealing the non-equilibrium distribution of CN in the
   atomizer, whereas C2 is only marginally affected. Spatially resolved
   measurements of Al also indicate the importance of O2 ingress and the
   sheath gas flow rate on the Al atomic absorption signals, indicating
   that the pO2 largely determines the degree of Al atom formation and not
   the presence of gaseous, carbon-containing species.},
DOI = {10.1039/ja9920701063}
}


@article{92BlZaxx.CN,
Author = {Black, J. F. and Zare, R. N.},
Title = {{Observation of perturbations in the rotational manifold of the CN B 2-SIGMA+ UPSILON = 1 level caused by interaction with the CN A 2-PI-I
   UPSILON = 12 level}},
journal = JCSFT,
Year = {1992},
Volume = {88},
Pages = {525-529},
Abstract = {The (1, 0) band of the CN B2-SIGMA+-X2-SIGMA+ transition has been
   examined using sub-Doppler laser-induced fluorescence (LIF), and the
   spin-rotation splitting of the upper state was found to vary
   irregularly. This behaviour is identified as arising from localised
   perturbations of the CN B2-SIGMA+ (v = 1) level caused by interaction
   with levels of the same total angular momentum arising from the v = 12
   vibrational level of the A 2-PI(i) state. A deperturbation analysis
   based on diagonalising the Hamiltonian of the B2-SIGMA+ and A 2-PI(i)
   states including their mutual interaction is able to match well the
   experimentally observed separations between the F1(J = N + 1/2) and
   F2;(J = N - 1/2) levels of the B2-SIGMA+ state},
DOI = {10.1039/ft9928800525}
}


@article{92Kurucz.CN,
Author = {Kurucz, R. L.},
Title = {{Atomic and molecular-data for opacity calculations}},
Journal = {Rev. Mex. Astron. Astrofis.},
Year = {1992},
Volume = {23},
Pages = {45-48},
Abstract = {I am attempting to produce line lists for all atoms and diatomic
   molecules that are important in stars. I collect all published data on
   spectrum analysis and oscillator strengths. I compute the energy levels,
   wavelengths, gf values, and damping constants that are not available
   from the literature. Line lists have been computed for diatomic
   molecules H-2, CH, NH, OH, MgH, SiH, CN, C2, CO, SiO, and TiO, and for
   the iron group atoms Ca I-IX to Ni I-IX. These lists total 58 million
   lines. These calculations are being revised as new laboratory data
   become available. The work is being extended to other diatomic
   molecules, to lighter and heavier elements, and to higher stages of
   ionization.},
url = {http://adsabs.harvard.edu/pdf/1992RMxAA..23...45K}
}


@article{92Zyrnicki.CN,
Author = {Zyrnicki, W.},
Title = {{Vibrational temperatures of some molecules in a hollow-cathode discharge}},
Journal = {Spectr. Lett.},
Year = {1992},
Volume = {25},
Pages = {175-188},
Abstract = {Investigation of vibrational energy distributions of molecules excited
   in a hollow cathode discharge has been presented here.  The vibrational
   temperatures of the CN, N2 and N2+ molecules have been measured at
   different experimental conditions.  The excitation processes in HCD have
   been discussed.},
DOI = {10.1080/00387019208020685}
}


@article{91Blvaxx.CN,
Author = {Black, J. H. and van Dishoeck, E. F.},
Title = {{Electron-densities and the excitation of CN in molecular clouds}},
Journal = ApJ,
Year = {1991},
Volume = {369},
Pages = {L9-L12},
Abstract = {In molecular clouds of modest density and relatively high fractional
   ionization {[}n(e)/n(H-2) > 10(-5)], the rotational excitation of CN is
   controlled by a competition among electron impact, neutral impact
   (primarily H-2), and the interaction with the cosmic background
   radiation.  The degree of excitation can be measured through optical
   absorption lines and millimeter-wave emission lines.  The available,
   accurate data on CN in diffuse and translucent molecular clouds are
   assembled and used to determine electron densities.  The derived values,
   n(e) almost-equal-to 0.02-0.5 cm-3, imply modest neutral densities in
   the range 10(2.5) - 10(3.5) cm-3, which generally agree well with
   determinations by other techniques.  The absorption- and emission-line
   measurements of CN both exclude densities higher than n(H-2)
   almost-equal-to 10(3.5) cm-3 on scales varying from 10(-3) to 60{''} in
   these clouds.},
DOI = {10.1086/185947}
}


@article{91WiHiKe.CN,
Author = {Wiedemann, G. R. and Hinkle, K. H. and Keady, J. J. and Deming, D. and Jennings, D. E.},
Title = {{CN and HCN in the infrared-spectrum of IRC+10216}},
Journal = ApJ,
Year = {1991},
Volume = {382},
Pages = {321-326},
Abstract = {The abundance of HCN in the inner circumstellar shell of IRC + 10216 has
   been remeasured using the 12-mu-m nu-2 band.  The 12-mu-m lines are less
   saturated than HCN 3-mu-m lines previously detected in the spectrum of
   IRC + 10216.  The observed 12-mu-m HCN line is formed in the
   circumstellar shell from about 4 to 12 R{*} in accord with a
   photospheric origin for HCN.  The derived HCN abundance in the 4 to 12
   R{*} region is 4 x 10(-5) and the column density is 7 x 10(18) cm-2.
   The 5-mu-m CN vibration-rotation fundamental band was detected for the
   first time in an astronomical source.  Using four CN lines, the CN
   column density was determined to be 2.6 x 10(15) cm-2 and the rotational
   temperature to be 8 +/- 2 K.  The peak radial abundance is 1 x 10(-5).
   The values for the temperature and abundance are in good agreement with
   microwave results and with the formation of CN from the photolysis of
   HCN.},
DOI = {10.1086/170719}
}


@article{91ItKuYa.CN,
Author = {Ito, H. and Kuchitsu, K. and Yamamoto, S. and Saito, S.},
Title = {{Microwave spectroscopy of the UPSILON=3-10 levels of CN(X2-SIGMA+)}},
Journal = CPL,
Year = {1991},
Volume = {186},
Pages = {539-546},
Abstract = {The fine-structure and hyperfine-coupling constants for the (CN)-C-12(X
   2-SIGMA+), upsilon = 3-10 levels were determined by an analysis of the
   microwave spectrum. The second-order perturbation treatment for the
   spin-rotation interaction constants, gamma-upsilon, is found to be
   sufficient for the (CN)-C-12(X 2-SIGMA+), upsilon = 5-10 levels. The
   transition frequencies for the upsilon = 4-10 levels are analyzed by
   including the X 2-SIGMA+ approximately A 2-PI(i) local perturbations in
   the fine-structure Hamiltonian matrix. The gamma-upsilon constants thus
   obtained fit the second-order expression. The electronic terms of the X
   2-SIGMA+ approximately A 2-PI(i) spin-orbit and rotational-electronic
   interactions are determined as functions of the internuclear distance.},
DOI = {10.1016/0009-2614(91)90464-K}
}


@article{91KaItSu.CN,
Author = {Kanda, K. and Ito, H. and Suzuki, K. and Kondow, T. and Kuchitsu, K.},
Title = {{Formation of CN (A2-PI-I) fragment from BRCN in an Argon flowing
   afterglow}},
Journal = CPL,
Year = {1991},
Volume = {185},
Pages = {225-230},
Abstract = {The vibrational bands of upsilon' = 3-22 of the CN(A PI-1-X 2-SIGMA+)
   emission produced in the dissociative excitation of BrCN with Ar(3P0.2)
   were observed in the region 460-750 nm by a flowing-afterglow method
   under essentially collision-free conditions in order to investigate the
   formation dynamics of the CN(A 2-PI-i) fragment.  The nascent
   vibrational distribution of the A 2-PI-i species was determined by a
   simulation analysis of the CN (A 2-PI-i-X 2-SIGMA+) emission.  A
   surprisal analysis of the vibrational distribution shows that the
   dominant mechanism producing the CN (A 2-PI-i) radical is energy
   transfer, being in parallel with that of the reaction producing the CN
   (B 2-SIGMA+) radical.},
DOI = {10.1016/S0009-2614(91)85052-X}
}


@article{91KoZiGr.CN,
Author = {Kolos, R. and Zielinski, Z. and Grabowski, Z. R. and Mizerski, T.},
Title = {{Dicyanoacetylene photodissociation at 193-nm and at 248-nm studied by
   transient absorption-spectroscopy - production of CN, C2 and C3N}},
Journal = CPL,
Year = {1991},
Volume = {180},
Pages = {73-80},
Abstract = {Laser photolysis of dicyanoacetylene at 193 nm and at 248 nm, leading to
   the production of CN, C2 and (probably) C3N, has been investigated.  CN
   violet bands and C2 Swan bands were observed in absorption.  Photolysis
   at 193 nm produces CN in both the X 2-SIGMA+ and in the A 2-PI-i
   manifolds, the latter - probably populated via a two-photon process -
   being revealed by a delayed (collision-induced) population of higher
   vibrational levels of the ground electronic state.  Photolysis at 248 nm
   is an efficient two-photon process.  It is still not clear what
   mechanism is responsible for the formation of the C2 molecule at 193 nm.
   Throughout the paper, the theoretical results of Sadlej and Roos helped
   to choose the optimal interpretation of the experiments.},
DOI = {10.1016/0009-2614(91)87119-V}
}


@article{91DaMcxx.CN,
Author = {Darrach, M. and McConkey, J. W.},
Title = {{Laser-induced fluorescence measurements of the X2-SIGMA+ state of CN
   produced by electron-impact on CH3CN}},
Journal = JCP,
Year = {1991},
Volume = {95},
Pages = {754-760},
Abstract = {Laser induced fluorescence (LIF) measurements of CN production following
   electron impact on CH3CN are carried out over an energy range from 30 to
   300 eV.  Cross sections for production of CN (X, upsilon
   approximately-equal-to 0) over this range are obtained using the cross
   section for production of N2+ (X, upsilon = 0) as standard.  The CN
   fragments are determined to be rotationally ``cold{''} suggesting that
   dissociation occurs by stretching of the C-C bond.  Rotational
   ``temperatures{''} increased by a factor of 2 (to 80 K) as the incident
   electron energy was increased from 30 to 300 eV.  LIF measurements of
   the translational energies of the CN fragments revealed a group with a
   surprisingly low translational temperature of 130 K.},
DOI = {10.1063/1.461081}
}


@article{91ItSuKo.CN,
Author = {Ito, H. and Suzuki, K. and Kondow, T. and Kuchitsu, K.},
Title = {{Electronic-transition moment for the B2-SIGMA+-X2-SIGMA+ emission of CN
   - Analysis of dependence on the internuclear distance}},
Journal = JCP,
Year = {1991},
Volume = {94},
Pages = {5353-5359},
Abstract = {The dependence of the electronic transition moment R(e)(R) for the
   B2-SIGMA+-X2-SIGMA+ transition of CN on the internuclear distance R is
   determined by an analysis of the relative intensity of its emission
   spectrum.  In the present analysis of the spectrum, the relative values
   of R(e)(R) are directly evaluated by use of the vibrational integrals,
   <upsilon'\textbackslash{}R(n)\textbackslash{}upsilon{''}> (n = 1,2,...),
   instead of the r-centroid approximation, which is found to be inadequate
   particularly in the tail bands (upsilon' greater-than-or-equal-to 10).
   The validity of the r-centroid approximation is discussed on the basis
   of the Rydberg-Klein-Rees (RKR) potentials of the B2-SIGMA+ and
   X2-SIGMA+ states.  The R dependence of the R(e)(R) value is interpreted
   in terms of the electronic wave function of B2-SIGMA+; the
   configuration-interaction (CI) coefficients for the electronic
   configurations of the B2-SIGMA+ state are estimated.  The CI
   coefficients obtained in the present study, as well as those for the
   A2-PI-i state derived previously from a similar analysis of the
   electronic part of the spin-orbit constant A(R) for A2-PI-i, are used to
   evaluate the electronic part of the spin-orbit matrix element between
   the B2-SIGMA+ and A2-PI-i states H(el)SO.  The H(el)SO values are
   predicted to be -25 and -21 cm-1 at R = 0.97 and 1.3 angstrom,
   respectively, being consistent with those determined experimentally in
