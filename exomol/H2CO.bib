@article{17HoMuLe.H2CO,
Author = {Holger, S. P. and Müller, Frank Lewen},
Title = {{Submillimeter spectroscopy of H2C17O and a revisit of the rotational
spectra of H2C18O and H2C16O}},
Journal = JMS,
year = {2017},
Volume = {331},
Pages = {28-33},
Abstract = {The rotational spectrum of the formaldehyde isotopologue H2C17O
was investigated between 0.56 and 1.50THz using a sample of natural
