@article{17FjHeBa.H2CO,
Author = {Fjodorow, Peter and Hellmig, Ortwin and Baev, Valery M. and Levinsky,
   Howard B. and Mokhov, Anatoli V.},
Title = {{Intracavity absorption spectroscopy of formaldehyde from 6230 to 6420
   cm(-1)}},
Journal = APB-LO,
Year = {2017},
Volume = {123},
pages = {147},
Abstract = {We apply intracavity absorption spectroscopy for measurements of the
   absorption spectrum of formaldehyde, CH2O, from 6230 to 6420 cm(-1), of
   which only a small fraction (6351-6362 cm(-1)) has been recorded
   elsewhere. The measurements are performed in the cavity of a broadband
   Er3+-doped fiber laser, with a sensitivity corresponding to the
   effective absorption path length of 45 m and a spectral resolution of
   0.1 cm(-1). The noise-equivalent detection limit of CH2O achieved with
   the strongest absorption line at 6252.64 cm(-1) is estimated to be 5
   ppm. High tolerance to broadband losses and the accessible time
   resolution of 50 mu s make it possible to apply this detection system
   for time-resolved monitoring of CH2O together with other molecules in
   harsh combustion environments, e.g., in combustion engines.},
DOI = {10.1007/s00340-017-6725-8}
}


@article{17TaAdNg.H2CO,
author = {T. L. Tan and Rabia'tul A'dawiah and L. L. Ng},
title = {{The 2ν2 bands of H212CO and H213CO by high-resolution FTIR spectroscopy}},
journal = JMS,
year = {2017},
volume = {340},
pages = {16-20},
abstract = {The Fourier transform infrared (FTIR) absorption spectra of the 2ν2
overtone bands of formaldehyde H212CO and its isotopologue H213CO were recorded
at an unapodized resolution of 0.0063cm−1 in the 3300–3540cm−1 region. Upper
state (v2=2) rovibrational up to two sextic centrifugal distortion constants
were accurately determined for both H212CO and H213CO. A total of 533 unperturbed
infrared transitions of H212CO and 466 unperturbed infrared transitions of H212CO
were assigned and fitted with rms deviations of 0.0012cm−1 and 0.00084cm−1 respectively
using Watson’sA-reduced Hamiltonian in the Ir representation. Analysis of new transitions
for H212CO measured in this work yielded upper state constants with greater accuracy
than previously reported. The infrared transitions of the 2ν2 band of H213CO were
measured for the first time. The band center of the A-type 2ν2 band of H212CO was
found to be 3471.71403±0.00012cm−1 and that of H213CO was 3396.628983±0.000083cm−1.
Furthermore, the newly assigned high-resolution infrared lines of the 2ν2 bands in
the 3300–3540cm−1 region can be useful in detecting the H212CO and H213CO molecules
in this IR region.},
keywords = {Formaldehyde molecules, HCO and HCO, Formaldehyde isotopologues,
High-resolution FTIR spectrum, Rovibrational constants, Infrared spectrum},
doi = {10.1016/j.jms.2017.07.013}
}

@article{2015AlYaTe.H2CO,
  title = {{{ExoMol}} Line Lists \textendash{} {{VIII}}. {{A}} Variationally Computed Line List for Hot Formaldehyde},
