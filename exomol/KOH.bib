@article{88WhPeBr.KOH,
Author = {White, M. A. and Perrott, A. and Britten, D. and Vanoort, M. J. M.},
Title = {{Thermodynamic characterization of the low-temperature phase-transformations
in KOH and KOD}},
Journal = JCP,
Year = {1988},
Volume = {89},
Number = {7},
Pages = {4346-4348},
abstract = {The heat capacities of KOH and its deuterated analog KOD have been
measured over the temperature range 17<T<342 K and 32<T<295 K, respectively. The
thermodynamics of the phase transformations to the low‐temperature antiferroelectric
phases have been characterized: Ttr (KOH)=226.7±0.2 K; Ttr (KOD)=253.1±0.1 K; ΔStr
(KOH)=0.121±0.002 R; ΔStr (KOD)=0.1258±0.0007 R. The thermodynamic changes are
consistent with tunneling of the hydroxide (deuteroxide) ions between two minima
in a double‐well potential.},
DOI = {10.1063/1.454819}
}


@article{87RaYaGia.KOH,
