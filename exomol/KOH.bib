@article{77KuToDy.KOH,
author = {Kuijpers, P. and Törring, T. and Dymanus, A.},
title = {{Quadrupole hyperfine structure and splitting of Δ-Levels in the microwave
spectra of KOH, RbOH and CsOH in the 100 GHz region.}},
journal = {Z. Naturforsch. A},
year = {1977},
volume = {32},
pages = {930-934},
abstract = {Hyperfine structure of rotational transitions of KOH, RbOH and CsOH
in various υ2-and l-states has been carefully measured in the 100 GHz range. From
the observed splittings and broadenings information about quadrupole coupling constant
(eqQ) of the K nucleus in KOH and about the spacing (EΔ - EΣ) between Σ and Δ levels
in the vibrational spectrum of KOH, RbOH and CsOH has been derived. The measured
value of the eqQ of KOH is close to that of KF. The separation between Σ and Δ
levels is found to be rather similar for the group of the alkali hydroxides in­
creasing gradually when progressing from LiOH to CsOH.},
doi = {10.1515/zna-1977-0904}
}


@ARTICLE{76PeWiTr.KOH,
author = {Pearson, E. F. and Winnerwisser, B. P. and Trueblood, M. B.},
title = {{Millimeter wave spectrum of excited vibrational states of potassium hydroxide. KOH and KOD}},
journal = {Z. Naturforsch. A},
year = {1976},
volume = {31},
number = {10},
pages = {1259-1267},
abstract = {Rotational transitions in the millimeter wave region are reported
for a number of excited vibrational states of KOH and KOD. From an analysis of
the J-dependence of the l-type resonance interactions the separation of the l
sublevels of several ν1 ν2l 0 states has been obtained. The cubic anharmonic
contributions to α1 and α2 were determined and the sign and isotopic variation
of α2 are shown to be consistent with a nearly harmonic bending potential function.
An re structure has been determined from the data using a calculated value for α3 .
On the basis of the present data, a severely anharmonic bending potential function
for KOH can be excluded. Our results are compared with those for the heavier alkali
hydroxides.},
doi = {10.1515/zna-1976-1017}
}

