@article{   98MaDaRe.NO,
   author = {Mandin, J. Y. and Dana, V. and Regalia, L. and Barbe, A. and {Von der Heyden}, P.},
   title = {Lambda-splittings and line intensities in the 3 <- 1 hot band of (NO)-N-14-O-16: The spectrum of nitric oxide in the first overtone region},
   journal = JMS,
   volume = {187},
   pages = {200-205},
   abstract = {Intensities and Lambda-splittings have been measured for lines belonging to the 3 <-- 1 hot band of (NO)-N-14-O-16. The vibrational transition moment and the Herman-Wallis coefficient have been determined. Hamiltonian constants have been calculated for the four vibrational states v = 0, 1, 2, and 3, and a synthetic spectrum has been generated in the first overtone region of NO, taking into account the hyperfine structure for the 2 <-- 0 band. (C) 1998 Academic Press.},
