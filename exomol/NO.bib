@article{   94MaDaCo.NO,
   author = {Mandin, J. Y. and Dana, V. and Coudert, L. H. and Badaoui, M. and Leroy, F. and Morillonchapey, M. and Farrenq, R. and Guelachvili, G.},
   title = {LINE POSITIONS AND INTENSITIES IN THE FUNDAMENTAL 1 -0 FORBIDDEN (2)PI(1/2)-(2)PI(3/2)-SUBBAND AND (2)PI(3/2)-(2)PI(1/2)-SUBBAND OF NITRIC-OXIDE},
   journal = JMS,
   volume = {167},
   pages = {262-271},
   abstract = {Using Fourier transform spectra, absolute line positions and intensities have been measured in the (II1/2)-I-2 (II3/2)-I-2 and (II3/2)-I-2-(II1/2)-I-2 forbidden subbands of the vibrational I <-- 0 fundamental band of nitric oxide. To retrieve accurate results from the spectra, the hyperfine structure was carefully taken into account. From the measured line intensities, the vibrational transition dipole moment, as well as the Herman-Wallis coefficients, has been determined for the studied subbands. (C) 1994 Academic Press Inc.},
   DOI = {10.1006/jmsp.1994.1233},
