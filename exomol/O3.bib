Title = {{Femtosecond transient stimulated-emission pumping studies of ozone
   visible photodissociation}},
Journal = JCP,
Year = {1992},
Volume = {97},
Pages = {2149-2152},
DOI = {10.1063/1.463102}
}


@article{92BeDeDi.O3,
Author = {Bellini, M. and Denatale, P. and Dilonardo, G. and Fusina, L. and Inguscio,
   M. and Prevedelli, M.},
Title = {{Tunable far infrared-spectroscopy of O-16(3) ozone}},
Journal = JMS,
