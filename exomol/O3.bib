@article{92JoMaNe.O3,
Author = {Johnstone, W. M. and Mason, N. J. and Newell, W. R. and Biggs, P.
and Marston, G. and Wayne, R. P.},
Title = {{Differential oscillator-strengths produced by electron-impact
   energy-loss spectroscopy of ozone}},
Journal = JPB,
Year = {1992},
Volume = {25},
Pages = {3873-3883},
Abstract = {Electron impact spectroscopy has been used to obtain energy-loss spectra
   for ozone over an energy range 3.75-12.5 eV. The differential oscillator
   strength (DOS) is obtained from the O3 energy loss spectrum and compared
   with the DOS obtained from optical measurements. For energy losses up to
   9.75 eV the present work agrees well with the previous optical
   measurements but for higher values the DOS obtained in the present work
   is seen to deviate by up to 20\% from the values obtained from the
   optical data. Oscillator strengths for several transitions have also
   been calculated from a summation of the DOS and comparisons are made
   with optically and theoretically obtained values.},
DOI = {10.1088/0953-4075/25/18/013}
}


@article{92BaPlBoa.O3,
Author = {Barbe, A. and Plateaux, J. J. and Bouazza, S. and Flaud, J. M. and Camypeyret, C.},
Title = {{Spectral properties of ozone in the 5-mu-m region}},
Journal = JQSRT,
Year = {1992},
Volume = {48},
Pages = {599-610},
Abstract = {The region of 5 mum is one of the atmospheric windows where ozone may be
   easily observed in atmospheric spectra. It corresponds to the triad
   2nu3, nu1 + nu3, 2nu1, where nu1 + nu3 is the strongest band. Isotopic
   bands of nu1 + nu3, O-16O-16O-18 and O-16O-18O-16 may also be observed.
   This study reports fine positions, intensities, self broadening
   coefficients, air broadening coefficients and Shifts Of 03. These
   spectroscopic parameters are of interest for atmospheric measurements.
   Some of the results have been recently published, other are new and
   correspond to work in progress.},
DOI = {10.1016/0022-4073(92)90124-M}
}


@article{92DaBaSu.O3,
Author = {Dalgarno, A. and Babb, J. F. and Sun, Y.},
Title = {{Radiative association in planetary-atmospheres}},
Journal = PSS,
Year = {1992},
Volume = {40},
Pages = {243-246},
Abstract = {A summary account is presented of radiative association processes that
   may be significant in producing continuum emission in the atmospheres of
   the planets. The importance of the radiative association of N and O and
   of NO + O has been established by the presence in the airglow of the
   emission spectra. The possible contributions of O + O, O + O2 and O+ + O
   are discussed. The radiative association of He+ and H may be an
   important source of HeH+ in the Jovian ionospheres.},
DOI = {10.1016/0032-0633(92)90062-S}
}





@article{92FiJeCa.O3,
Author = {Fichet, P. and Jevais, J. R. and Camypeyret, C. and Flaud, J. M.},
Title = {{Nlte processes in ozone: Importance of O and O3 densities near the
   mesopause}},
Journal = PSS,
Year = {1992},
Volume = {40},
Pages = {989-1009},
Abstract = {Laboratory studies and atmospheric experiments have shown that Non Local
   Thermodynamic Equilibrium (NLTE) processes must be considered to explain
   the infrared emission of ozone above 70km for a quiescent atmosphere.
   For these altitudes, in the calculation of ozone limb infrared
   radiances, a complete kinetic model describing the coupling between
   radiative and relaxation processes must be used. In this study a
   narrow-band statistical radiative transfer model, combined with a
   kinetic model, is proposed providing calculations of NLTE infrared limb
   emission by ozone with good accuracy. Moreover, because the calculations
   are very fast, contrary to models based on line-by-line calculations, it
   has been possible to assess the influence of the various atmospheric
   input parameters which are of importance for modelling available
   experimental data such as SPIRE (Green et al, 1986).},
DOI = {10.1016/0032-0633(92)90140-J}
}


@article{92AnMaMo.O3,
Author = {Anderson, S. M. and Mauersberger, K. and Morton, J. and Schueler, B.},
Title = {{Heavy ozone anomaly: Evidence for a mysterious mechanism}},
Journal = {ACS Symp. Ser. Am. Chem. Soc.},
Year = {1992},
Volume = {502},
Pages = {155-166},
Abstract = {The unexpectedly large abundances of isotopically heavy ozone observed
   in a variety of environments indicates that our understanding of how
   ozone is formed is incomplete and may involve significant participation
   of one or more low-lying metastable electronic states. A summary of the
   experimental observations of ozone isotopomers in the stratosphere and
   in laboratory studies is presented. It is argued that these data are
   inconsistent with the standard picture of recombination reactions, and
   an alternative symmetry-selective mechanism which involves multiple
   electronic states is proposed. Recent progress in theoretical and
   experimental determinations of the energies of candidate states
   indicates that the B-3(2) state, and to a lesser extent the 3A2, may be
   involved.},
doi = {10.1021/bk-1992-0502.ch010}
}


@article{92Wayne.O3,
