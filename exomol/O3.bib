@article{93DaJoMa.O3,
Author = {Davies, J. A. and Johnstone, W. M. and Mason, N. J. and Biggs, P. and Wayne, R. P.},
Title = {{Vibrational-excitation of ozone by electron-impact}},
Journal = JPB,
Year = {1993},
Volume = {26},
Pages = {L767-L771},
Abstract = {The first measurements of electron impact vibrational excitation of
   ozone are presented as a function of both incident electron energy, 3 eV
   less-than-or-equal-to E(i) less-than-or-equal-to 7 eV, and scattering
   angle, 40-degrees less-than-or-equal-to theta less-than-or-equal-to
   120-degrees. Evidence is given for the formation of a low-lying shape
   resonance at E(i) almost equal to 4 eV. Preliminary vibrational
   excitation cross sections are also presented.},
DOI = {10.1088/0953-4075/26/21/008}
}


@article{93RoHaPl.O3,
Author = {Rosenmann, L. and Hartmann, J. M. and Plateaux, J. J. and Barbe, A.},
Title = {{Absorption in the Q-branch of the nu(1)+nu(2)nu(3) band of O3}},
Journal = JQSRT,
Year = {1993},
Volume = {50},
Pages = {233-235},
Abstract = {We have examined line-mixing in the Q-branch of the nu1+ nu2 + nu3 band
   of ozone perturbed by N2 at room temperature. Comparison between
   Fourier-transform and synthetic spectra has shown that the effect of
   line-mixing can be neglected in the computation of atmospheric spectra
   in the spectral region under investigation.},
DOI = {10.1016/0022-4073(93)90121-W}
}


