@article{91Zittel.O3,
Author = {Zittel, P. F.},
Title = {{Vibrational-relaxation and infrared-spectra of O-3 and OCS in cryogenic
   liquid solutions}},
Journal = JPC,
Year = {1991},
Volume = {95},
Pages = {6802-6811},
Abstract = {A laser-induced fluorescence method has been used to measure vibrational
   relaxation times for OCS and O3 in cryogenic liquid solutions. A CO2
   laser was used to pump the 2-nu-2 vibrational level of OCS dilute in
   various solvents, and fluorescence was observed from the nu-1 and nu-2
   levels. The fluorescence relaxation eigenvalues and a kinetic model gave
   relaxation probabilities per collision for the 2-nu-2, nu-1, and nu-2
   vibrational levels of OCS by Ar(l) at 84.4 K equal to > 1.1 x 10(-6),
   5.3(-1.1)+2.0 x 10(-7), and (2.2 +/- 0.2) x 10(-7), respectively. For
   Kr(l) at 116.8 K the respective probabilities were > 1.5 x 10(-6),
   4.3(-0.8)+1.7 x 10(-7), and (1.6 +/- 0.2) x 10(-7). For Xe(l) at 162.0 K
   the probabilities were > 1.1 x 10(-6), 2.9(-1.1)+1.8 x 10(-7), and (1.2
   +/- 0.1) x 10(-7), respectively. Relaxation of OCS by O2 was studied in
   mixtures of O2 in Ar(1), where the probabilities for relaxation of the
   nu-1 and nu-2 levels of OCS by O2 at 84.4 K were found to be
   2.6(-0.7)+1.1 x 10(-6) and (1.1 +/- 0.2) x 10(-6), respectively.
   Following CO2 laser excitation of the nu-3 vibrational level of O3
   dilute in the different solvents, fluorescence was observed from the
   nu-2 level. A lower limit of approximately 1 x 10(-6) was determined for
   the relaxation probability per collision of the equilibrated nu-1 and
   nu-3 stretching levels of O3 by each of the four collision partners. The
   probabilities for relaxation of O3(nu-2) by Ar(1) at 84.4 K, Kr(1) at
   116.8 K, Xe(1) at 162.0 K, and O2(dilute in Ar(1)) at 84.4 K were (1.6
   +/- 0.2) x 10(-7), (2.9 +/- 0.4) x 10(-7), > 6.9 x 10(-7), and (7.7 +/-
   0.9) x 10(-7), respectively. A comparison is made between the relaxation
   rates measured in the liquid phase at cryogenic temperatures and rates
   measured in the gas phase at room temperature, using the isolated binary
   collision model and energy-transfer theory. Fourier transform spectra
   were recorded for OCS and O3 in Ar(1), Kr(1), and Xe(1) solutions at the
   aforementioned temperatures and in O2(1) at 78.6 K. Band centers,
   half-widths, and strengths are given for eight vibrational bands of
   (OCS)-O-16-C-12-S-32 and six bands of O-16(3) in the spectral range of
   500-3500 cm-1. The ratio of the vibrational transition dipole moments of
   the nu-3 and nu-1 vibrations of O3, which are strongly Coriolis coupled
   in the gas phase, was measured to be mu-3/mu-1 = 13.2 +/- 1.6 in liquid
   Ar and O2 solutions.},
DOI = {10.1021/j100171a013}
}


@article{91ZuBaRe.O3,
Author = {Zuniga, J. and Bastida, A. and Requena, A. and Hidalgo, A.},
Title = {{Self-consistent-field calculation of vibrational bound-states for
   triatomic-molecules using transformed Jacobi coordinates}},
Journal = JPC,
Year = {1991},
Volume = {95},
Pages = {2292-2297},
Abstract = {The use of different transformed Jacobi coordinate systems for the
   self-consistent-field (SCF) calculation of vibrational energy levels of
   triatomic AB2 molecules is investigated.  These coordinate systems
   include those corresponding to the A-B2 and B-AB Jacobi arrangements and
   their partial transformations by rotating the radial coordinates.  The
   SCF coupled equations are solved by using the variational
   Harris-Engerholm-Gwinn pointwise method which can be easily implemented
   for the SCF treatment of general, nonmultinomial, potentials.  Numerical
   applications are made for the molecules of H2O, O3, and SO2 comparing
   the SCF results with those derived from full variational calculations.},
DOI = {10.1021/j100159a035}
}


@article{91Dawkins.O3,
Author = {Dawkins, A. W. J.},
Title = {{Measurement of stratospheric H2O and O-3 using a new balloon-borne
   Rremote-sensing technique}},
Journal = JPD,
Year = {1991},
Volume = {24},
Pages = {1211-1220},
Abstract = {A new technique has been developed using a millimetre-wave radiometer to
   measure the concentration profiles of H2O and O3 in the lower
   stratosphere from a balloon platform.  The technique differs from
   previous microwave remote sensing techniques in being able to carry out
   measurements with much greater resolution, allowing it to resolve the
   finer structure in the vertical distributions of H2O and O3.  The first
   flight of the new instrument took place from the French launch site at
   Aire-sur-l'Adour during the 1983 MAP/GLOBUS campaign.  The basic
   principles of the new technique and the method of data analysis are
   described in the paper together with the results from the first flight.
   The results obtained were found to exhibit many of the important
   features seen by previous workers and are discussed in detail in the
   paper.},
DOI = {10.1088/0022-3727/24/7/029}
}


@article{91Schachter.O3,
Author = {Schachter, J.},
Title = {{Ozone absorption-bands in the 3100A - 3400A region}},
Journal = PASP,
Year = {1991},
Volume = {103},
Pages = {457-460},
Abstract = {I discuss the physical processes which produce terrestrial ozone
   absorption, deriving ozone opacities from laboratory cross sections and
   published ozone column densities.  The ozone opacity decreases from
   approximately 1 at 3100 angstrom to approximately 10(-3) at 3500
   angstrom.  A comparison of model DA white-dwarf spectra absorbed by
   ozone and actual observed spectra shows good agreement.  Therefore,
   simulated absorbed spectra may be used to remove ozone contamination in
   ground-based data.},
DOI = {10.1086/132842}
}


@article{91ScWeHe.O3,
Author = {Schinke, R. and Weide, K. and Heumann, B. and Engel, V.},
Title = {{Diffuse structures and periodic-orbits in the photodissociation of small
   polyatomic-molecules}},
Journal = {Faraday Discuss.},
Year = {1991},
Volume = {91},
Pages = {31-46},
Abstract = {The relation of diffuse vibrational structures in UV-absorption spectra
   of (small) polyatomic molecules and internal vibrational motion in
   excited electronic states are investigated. The method of choice is the
   propagation of time-dependent wavepackets with the autocorrelation
   function serving as the link between the energy dependence of the
   spectrum and the time dependence of the molecular motion in the excited
   electronic state. For the purpose of this paper we characterize diffuse
   structures as very short-lived resonances with `lifetimes' of the order
   of at most one internal vibrational period. In particular, we study a
   model system for the photodissociation of symmetric triatomic molecules
   ABA such as H2O, CO2 and O3, the photodissociation of H2O in the second
   continuum, and the fragmentation of H2S. The existence of unstable
   periodic orbits and their influence on the dissociation dynamics is
   especially elucidated. In the case of H2S we demonstrate that the
   diffuse absorption structures are caused by symmetric stretch motion in
   a binding state which is strongly coupled to a dissociative state.
   Diffuse structures can be regarded as very broad resonances in excited
   electronic states. They manifest transition-state spectroscopy in the
   original sense of the word.},
DOI = {10.1039/dc9919100031}
}


@article{91YaMoxx.O3,
Author = {Yamashita, K. and Morokuma, K.},
Title = {{Abinitio molecular-orbital and dynamics study of transition-state
   spectroscopy}},
Journal = {Faraday Discuss.},
Year = {1991},
Volume = {91},
Pages = {47-61},
Abstract = {Recent results on the ab initio potential-energy surface (PES) and
   dynamics calculations closely related to the experiments of
   `transition-state spectroscopy' (TSS) are discussed. The topics included
   are the photodissociations of NaI and ozone, and the unimolecular
   dissociation of FHF-. The nonadiabatic coupling in the ion-covalent
   crossing region of the NaI dissociation is calculated based on the
   CASSCF-MRCI method and compared with experimental and empirical
   estimates. The PESs of the ozone photodissociation are calculated using
   the CASSCF-SECI method. Characteristics of the ground and the excited
   B-state PESs are compared with the existing PESs. The weak structure
   seen in the Hartley band is explained in terms of transition-state
   dynamics based on three-dimensional quantum calculations using new PESs.
   The CEPA PES of the unimolecular dissociation of FHF- to the F- + HF
   channel is calculated. Based on the quantum-mechanical variational
   calculation, the vibrationally highly excited FHF- anion above its
   dissociation threshold is suggested as a candidate of TSS of
   unimolecular dissociation reactions without a barrier.},
DOI = {10.1039/dc9919100047}
}


@article{91Mlynczak.O3,
Author = {Mlynczak, M. G.},
Title = {{Nonlocal thermodynamic-equilibrium processes in ozone: Implications for
   the energy budget of the mesosphere and lower thermosphere}},
Journal = JGRA,
Year = {1991},
Volume = {96},
Pages = {17217-17228},
Abstract = {The conversion of chemical potential energy and infrared radiative
   energy to kinetic energy by nonlocal thermodynamic equilibrium (non-LTE)
   processes involving ozone is a potentially significant source of heat in
   the terrestrial upper mesosphere and lower thermosphere. Heating rates
   in excess of 6 K d-1 could occur near the mesopause if all potential
   energy is converted to heat. Calculation of the heating rate requires
   knowledge of the processes which convert chemical potential energy to
   internal energy of ozone during the exothermic recombination reaction
   that forms ozone. In addition, the rates at which ozone exchanges
   internal energy with its surroundings through collisional and radiative
   processes must be known. The lack of a detailed data base of kinetic and
   spectroscopic information limits the ability to accurately model the
   heating by ozone. Therefore heating rates are calculated and compared
   using two different statistical equilibrium models previously applied in
   the analysis of measurements of limb emission from ozone. The calculated
   heating depends strongly on the assumed distribution and relaxation of
   energy in the quasi-nascent ozone molecule. Finally, in the absence of a
   detailed data base of rate coefficients it may be possible to estimate
   the heating rate due to non-LTE processes in ozone from appropriate
   satellite measurements of the ozone concentration and of the infrared
   emission from ozone in the 9-12-mu-m spectral interval.},
DOI = {10.1029/91JD01833}
}


@book{90FlCaRi.O3,
author = {Flaud, J. M. and Camy-Peyret, C. and Rinsland, C. P. and Smith, M. A. H. and Devi, V. M.},
title = {{Atlas of ozone spectral parameters from microwave to medium infrared}},
year = {1990},
Address = {San Diego, CA},
Publisher = {Academic},
abstract = {At this point in time, an intense desire to understand fully the complex
process of the depletion of stratospheric ozone is pervading the scientific community
throughout the world. Laboratory experiments are being performed continually using
the full power of modern technology to generate unambiguous values for the molecular
parameters of ozone and related atmospheric constituents. These basic data are
essential for a reliable analysis of stratospheric spectra recorded with remote
sensing instruments. This book provides a critical assessment of our current
knowledge of ozone spectroscopy in the infrared and microwave regions. such a
comprehensive review, as embodied in this atlas, will certainly help in the planning
of new research required to achieve the desired precision in our experimental
knowledge of atmospheric constituents. In addition, it provides a solid base
necessary for the development of new sensors that will be useful in a global remote
sounding network.},
url = {https://www.osti.gov/biblio/5641054}
}


