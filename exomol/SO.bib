@article{ 22BeJoLib.SO,
Author = {Bernath, Peter F. and Johnson, Ryan M. and Lievin, Jacques},
Title = {{Line lists for (XE-)-E-3 and a(1)Delta vibration-rotation bands of SO}},
Journal = JQSRT,
Year = {2022},
Volume = {290},
Abstract = {SO is found in several astronomical sources such as the atmospheres of
   Io and Venus. To complete our work on SO line lists, we used our
   previous fits for v = 0 to v = 6 for the X-3 Sigma(-) state and v = 0-5
   for the a(1)Delta state {[}JQSRT 272, 107772 (2021)] for line positions,
   and high-level ab initio calculations of electric dipole moments for
   line strengths for the vibration-rotation bands. LeRoy's RKR program was
   used to produce pointwise potential energy curves. N- and v-dependent
   dipole matrix elements were produced with LeRoy's LEVEL program and the
   final line lists were made with Western's PGOPHER program. All possible
   vibrational bands were calculated, and the line strengths included the
   Herman-Wallis effect caused by vibration-rotation interaction.}},
DOI = {10.1016/j.jqsrt.2022.108317},
pages= {108317}}

@article{ 22BeJoLia.SO,
Author = {Bernath, Peter F. and Johnson, Ryan and Lievin, Jacques},
Title = {{Line lists for the b(1)Sigma(+)-X-3 Sigma(-) and a(1) Delta-X-3 Sigma(-)
   transitions of SO}},
Journal = JQSRT,
Year = {2021},
Volume = {272},
Abstract = {SO is found in many astronomical sources such as the atmospheres of Io
   and Venus. In order to create more complete line lists, we fit
   spectroscopic data on SO from the literature using PGOPHER. The fits
   covered v = 0 to v = 6 for the X-3 Sigma(-) state, v = 0-5 for the a(1)
   Delta state and v = 0-2 for b(1) Sigma(+) state. LeRoy's RKR program was
   used to produce pointwise potential energy curves. High level ab initio
   calculations, including spin-orbit coupling, were carried out to obtain
   the electric transition dipole moment functions for the nominally
   forbidden b(1)Sigma(+)-X-3 Sigma(-) and a(1) Delta-X-3 Sigma(-)
   transitions. The RKR potentials and transition dipole moment points were
   input into LeRoy's LEVEL program to calculate the transition dipole
   matrix elements for all possible b-X and a-X bands. For the b(1)
   Sigma(+) -X-3 Sigma(-) transition, the electric and magnetic transition
   dipole matrix elements were scaled using the experimental values
   obtained by Setzer et al. {[}J Mol Spectrosc 1999;198:163-174] for the
   0-0 band. The transition dipole moment matrix elements were used in
   PGOPHER to produce our line lists for all possible bands of the
   b(1)Sigma(+)-X-3 Sigma(-) and a(1) Delta-X-3 Sigma(-) transitions.},
DOI = {10.1016/j.jqsrt.2021.107772},
pages = {107772}
}

