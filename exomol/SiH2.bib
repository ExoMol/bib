@article{84RiHaxx.SiH2,
Author = {Rice, J. E. and Handy, N. C.},
Title = {{The low-lying states of silylene}},
Journal = CPL,
Year = {1984},
Volume = {107},
Number = {4-5},
Pages = {365-374},
abstract = {MC SCF calculations have been performed on the C2v surfaces of
the 1A1, 3B1, 1B1, 3A2, 1A2, 21A1, 3A1 and 3B2, states of silylene. These states
are important in the collisional removal of Si by molecular hydrogen. Of particular
interest are the barriers in the 3B1 and 1B1 surfaces, which are substantially
reduced on interaction with 3A2 and symmetry, and the small-angle minima of the
latter states.},
DOI = {10.1016/S0009-2614(84)80237-7}
}


