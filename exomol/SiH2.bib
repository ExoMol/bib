Journal = {J. Chem. Soc. Faraday Trans.},
Year = {1975},
Volume = {71},
Pages = {799-806},
abstract = {The rotational analysis of the adsorption spectra of SiH2 has been
extended to the 4–0, 5–0 and 6–0 bands. The two combining states, the X, 1A1 and
the Å, 1B1 states are shown to be derived from a 1Δ electronic state split by static
