@Article{94CoMcSt.SiH4,
  Title                    = {INFRARED INTENSITIES OF $\nu_3$ AND $\nu_4$ IN {SiH$_4$}, {GeH$_4$} AND {SnH$_4$}},
  Author                   = {Coats, A. M. and McKean, D. C. and Steele, D.},
  Journal                  = {J. Mol. Struct.},
  Year                     = {1994},
  Pages                    = {269-280},
  Volume                   = {320},

  Abstract                 = {New infrared intensity data for the bands owing to nu3 and nu4 in SiH4,
 GeH4 and SnH4 are reported. Substantial changes from earlier estimates
 are found, particularly for SnH4. The intensities in SiH4 are also
 calculated ab initio using 6-31G{*} and 6-31G{*}{*} basis sets. With the
 signs of partial derivative p/partial derivative Q fixed by these and
 prior calculations, partial derivative p/partial derivative S values are
 calculated and interpreted in terms of zero order electro optical
 parameters, charge fluxes and atomic polar tensors. The atom
 polarisation P(A) found for SiH4 agrees well with that from refractive
 index and dielectric constant measurements. The infrared intensities
 found correlate well with the frequency shifts from gas to high
 temperature crystal phase. The Haas-Ketelaar equation accounts for 45\%
 and 70\% respectively of these shifts in nu3 and nu4, after appropriate
 estimates of density and refractive index have been made.},
