@article{92ScSeCa.TiO,
  author = {Schamps, J. and Sennesal, J. M. and Carette, P.},
  title = {{Spontaneous radiative properties of the excited electronic states of TiO}},
  journal = JQSRT,
  year = {1992},
  volume = {48},
  pages = {147-152},
  abstract = {Ab initio electronic wavefunctions have been used to calculate and
