@article{93FlScJu.TiO,
  author = {Fletcher, D. A. and Scurlock, C. T. and Jung, K. Y. and Steimle, T. C.},
  title = {{Hyperfine interactions in the ground-states of titanium monoxide
  and mononitride}},
  journal = JCP,
  year = {1993},
  volume = {99},
  pages = {4288-4299},
  abstract = {A comparative study of the hyperfine interactions in the X 2SIGMA+
  state of TiN and the X 3A state of TiO has been performed. The (TiN)-Ti-48-N-14(I
  = 1) hyperfine structure was determined from the analysis of 19 components
  of the N = 1-0 and N = 2-1 pure rotational transitions recorded using
  the pump/probe microwave-optical double resonance technique. The
  Ti-47(I = 5/2) hyperfine structure of X 2SIGMA+ TiN was determined
  from an analysis of the high resolution optical spectrum of the (0,0)
  A 2PI3/2-X2SIGMA+ band system. The resulting parameters are (in MHz)
  B((TiN)-Ti-48-N-14) = 18 589.3513 (13), D((TiN)-Ti-48-N-14) = 0.026
  31 (18), gamma ((TiN)-Ti-48-N-14) = -52.2070 (13), b(F)(N) = 18.480(3),
  c(N) = 0.166(7), eQq0(N) = -1.514(8), C(I)(N) = 0.0137(12), b(F)(Ti-47)
  = -558.8(11), c(Ti-47) = -15(5), and eQq0(Ti-47) = 62(16). An analysis
  of the (0,0) band of the B 3PI-X 3DELTA system of (TiO)-Ti-47-O-16
  produced the X 3DELTA hyperfine parameters (in MHz): a (Ti-27) =
  -54.7(21), (b(F) + 2c/3) (Ti-47) = -231.6(60), and eQq0(Ti-47) =
  -49(31). An interpretation based upon the predicted nature of the
  bonding in TiO and TiN is given.},
  doi = {10.1063/1.466082},
  keywords = {Ab initio}
}

