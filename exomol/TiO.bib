@ARTICLE{09TaMuBl.TiO,
  author = {Tamanai, A and Mutschke, H and Blum, J and Posch, T and Koike, C and Ferguson, J W},
  title = {{Morphological effects on IR band profiles Experimental spectroscopic
	analysis with application to observed spectra of oxygen-rich AGB
	stars}},
  journal = AA,
  year = {2009},
  volume = {501},
  pages = {251-267},
  abstract = {Aims. To trace the source of the unique 13, 19.5, and 28 mu m emission
	features in the spectra of oxygen-rich circumstellar shells around
	AGB stars, we have compared dust extinction spectra obtained by aerosol
	measurements. Methods. We have measured the extinction spectra for
	19 oxide powder samples of eight different types, such as Ti-compounds
	(TiO, TiO(2), Ti(2)O(3), Ti(3)O(5), Al(2)TiO(5), CaTiO(3)), alpha-,
	gamma-, chi-delta-kappa-Al(2)O(3), and MgAl(2)O(4) in the infrared
	region (10-50 mu m) paying special attention to the morphological
