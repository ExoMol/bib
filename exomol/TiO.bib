@ARTICLE{94MiBaxx.TiO,
  author = {Milone, A and Barbuy, B},
  title = {{TIO BANDS AS A FUNCTION OF STELLAR PARAMETERS}},
  journal = AAS,
  year = {1994},
  volume = {108},
  pages = {449-454},
  abstract = {TiO bands at lambda lambda 705 - 745, 615 - 630, 594 - 600 nm are
	clean strong features in metal-rich systems such as metal-rich globular
	clusters and normal galaxies. In the present work, we study the behaviour
	of TiO bands as a function of stellar parameters for individual stars
	trying to establish a calibration of TiO bands intensity vs stellar
	parameters. These features can be used together with more usual ones
	such as Mg-2 to better derive metallicities.},
  keywords = {Astrophysics; LATE-TYPE STARS; STARS, FUNDAMENTAL PARAMETERS},
