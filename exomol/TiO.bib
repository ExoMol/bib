@ARTICLE{05ViStBr.TiO,
  author = {Virgo, W L and Steimle, T C and Brown, J M},
  title = {{Optical Zeeman spectroscopy of the (0,0) bands of the B (3)Pi-X-3
	Delta and A (3)Phi-X-3 Delta transitions of titanium monoxide, tio}},
  journal = ApJ,
  year = {2005},
  volume = {628},
  pages = {567-573},
  abstract = {The Zeeman effect on lines in the (0, 0) band of the B(3)Pi-X(3)Delta(gamma')
	system and in the (0, 0) band of the A(3)phi-X(3)Delta (gamma) system
	of titanium monoxide, TiO, have been recorded and analyzed. Magnetic
	tuning of the low rotational spectral features recorded at high resolution
	( FWHM similar or equal to 35 MHz) and at field strengths of up to
	1.2 kG is accurately modeled using an effective Zeeman Hamiltonian.
	A comparison is made with previous predictions, which were based
	on field-free spectroscopic parameters. It is shown that the large
	Lambda-doubling in the B(3)Pi state necessitates the inclusion of
	Delta J = +/- 1 matrix elements in the representation of the effective
	Hamiltonian operator. The observed spectra could only be modeled
	by allowing the electronic spin and orbital magnetic g-factors, g(S)
	and g(L), of the B(3)Pi and A(3)phi states to deviate from 2.002
	and 1.000. Nonadiabatic mixing with the nearby C(3)Delta state is
	proposed as the cause of these deviations.},
  doi = {10.1086/431282},
  keywords = {Astrophysics; astrochemistry; molecular data; molecular processes;
	stars: late-type; stars: magnetic fields},
  keywords-plus = {STELLAR MAGNETIC-FIELDS; SUNSPOT SPECTRA; SOLAR; SYSTEM; BANDS; LINES;
	DIAGNOSTICS; ABSORPTION; RESOLUTION; MOLECULES}
}

