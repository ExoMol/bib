@article{92BeReSh.TiO,
  author = {Berdyugina, S. V. and Petrov, P. P. and Shcherbakov, V. A.},
  title = {{Search for TiO-band variability in the spectrum of the T-Tauri star
  V410-TAU}},
  journal = {Sov. Astron. Lett.},
  year = {1992},
  volume = {18},
  pages = {443-445},
  abstract = {In 1991-1992, we observed the spectrum of the star V410 Tau in the
  vicinity of the TiO bands near 7100 angstrom with low spectral resolution
  and high signal-to-noise ratio. The star's brightness is correlated
  with the strength of the TiO bands, confirming the suspected existence
  of cool spots on the star's surface. We model this relationship using
  synthetic spectra of the photosphere and spots.},
  url = {http://www3.leibniz-kis.de/~sveta/papers/v410tau1.pdf},
  keywords = {Astrophysics}
