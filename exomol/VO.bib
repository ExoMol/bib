@article{23JoWaAs.VO,
Author = {Johnson, Marshall C. and Wang, Ji and Asnodkar, Anusha Pai and Bonomo,
   Aldo S. and Gaudi, B. Scott and Henning, Thomas and Ilyin, Ilya and
   Keles, Engin and Malavolta, Luca and Mallonn, Matthias and
   Molaverdikhani, Karan and Nascimbeni, Valerio and Patience, Jennifer and
   Poppenhaeger, Katja and Scandariato, Gaetano and Schlawin, Everett and
   Shkolnik, Evgenya and Sicilia, Daniela and Sozzetti, Alessandro and
   Strassmeier, Klaus G. and Veillet, Christian and Yan, Fei},
Title = {{The PEPSI Exoplanet Transit Survey (PETS). II. A Deep Search for Thermal
   Inversion Agents in KELT-20 b/MASCARA-2 b with Emission and Transmission
   Spectroscopy}},
Journal = AJ,
Year = {2023},
Volume = {165},
Abstract = {Recent observations have shown that the atmospheres of ultrahot Jupiters
   (UHJs) commonly possess temperature inversions, where the temperature
   increases with increasing altitude. Nonetheless, which opacity sources
   are responsible for the presence of these inversions remains largely
   observationally unconstrained. We used LBT/PEPSI to observe the
   atmosphere of the UHJ KELT-20 b in both transmission and emission in
   order to search for molecular agents which could be responsible for the
   temperature inversion. We validate our methodology by confirming a
   previous detection of Fe i in emission at 16.9 sigma. Our search for the
   inversion agents TiO, VO, FeH, and CaH results in non-detections. Using
   injection-recovery testing we set 4 sigma upper limits upon the volume
   mixing ratios for these constituents as low as similar to 1 x 10(-9) for
   TiO. For TiO, VO, and CaH, our limits are much lower than expectations
   from an equilibrium chemical model, while we cannot set constraining
   limits on FeH with our data. We thus rule out TiO and CaH as the source
   of the temperature inversion in KELT-20 b, and VO only if the line lists
   are sufficiently accurate.},
DOI = {10.3847/1538-3881/acb7e2},
pages = {157}
}

