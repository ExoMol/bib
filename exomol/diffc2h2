< @article{93WeBlNa.C2H2,
< author = {Mark Weber and William E Blass and Shachar Nadler and George W Halsey and William C Maguire and John J Hillman},
< title = {{Resonance effects in C$_2$H$_2$ near 13.7 $\mu$m. Part H: The two quantum hotbands}},
< Journal = SCAA,
---
> @article{93WeBlNa.c2h2,
> author = {Weber, M. and Blass, W. E. and Nadler, S. and Halsey, G. W. and Maguire, W. C. and Hillman, J. J.},
> title = {l-Resonance effects in {C2H2} near 13.7$mu$m. Part {II}: The two quantum hotbands},
> Journal = {Spectrochimica Acta Part A: Molecular Spectroscopy},
1204,1205c1675,1676
< doi = {10.1016/0584-8539(93)80124-S},
< abstract = {This article is the second part on ℓ-resonance effects on the rotation-vibration bands of acetylene observed in the ν5 fundamental region. While the first part concentrated on the energy level analysis of the fundamental and the seven strongest hotbands originating in the ν4 and ν5 excited states for both major isotopes [Spectrochim. Acta 48A, 1203 (1992)], this article summarizes the results of the analysis of the hotbands 2ν4 + ν5 ← 2ν4, ν4 + 2ν5 ← ν4 + ν5, and 3ν5 ← 2ν5 from which improved molecular constants for the 2ν4 and three quantum energy levels were derived for the major isotope $^{12}$C$_2$H$_2$. The mixing levels within the excited vibrational states due to vibrational and rotational ℓ-resonance effects are discussed which lead to the identification of the strong “forbidden” Δℓ 3 band, 2ν4+ ν35←2νOe4 as a result of ℓ-resonance intensity perturbation.}
---
> URL = {http://dx.doi.org/10.1016/0584-8539(93)80124-S},
> doi = {10.1016/0584-8539(93)80124-S}
1208,1211c1679,1682
< @article{94WeBlHa.C2H2,
< author = {M. Weber and W. E. Blass and G. W. Halsey and J. J. Hillman},
< title = {I-Resonance Perturbation of IR Intensities in $^{12}$C$_2$H$_2$ near 13.7 μm},
< Journal = JMS,
---
> @article{94WeBlHa.c2h2,
> author = {M. Weber and W.E. Blass and G.W. Halsey and J.J. Hillman},
> title = {I-Resonance Perturbation of IR Intensities in 12C2H2 near 13.7 μm},
> Journal = {J. Mol. Spectrosc.},
1212a1684
> number = {1},
1214a1687
> issn = {0022-2852},
1216c1689,1690
< abstract = {Using TDL and FT spectrometric data at 0.0005 and 0.0025 cm−1 spectral resolution, respectively, individual line intensities of the ν5 fundamental and the seven strongest hot bands, ν4 + ν5 ← ν4 and 2ν5 ← ν5, of $^{12}$C$_2$H$_2$ in the 13.7-μm region have been measured. Except for the 2ν2f5 ← ν1f5 hot band, strong intensity perturbation due to l-type doubling and resonances have been quantitatively determined for all hot bands. The zeroth-order vibrational dipole moment derivative for the ν5 bending mode is identical within 10$\%$ for all bands observed. Since an extra factor of 12 appears in the square vibrational transition moment for the combination band, its intensity is reduced by half and is comparable in magnitude to the 2ν5 ← ν5 hot bands. For the ν5 fundamental a square dipole moment derivative of Γ2v = 0.09069(24)D2 has been determined.}
---
> url = {//www.sciencedirect.com/science/article/pii/S0022285284711143},
> abstract = {Using TDL and FT spectrometric data at 0.0005 and 0.0025 cm−1 spectral resolution, respectively, individual line intensities of the ν5 fundamental and the seven strongest hot bands, ν4 + ν5 ← ν4 and 2ν5 ← ν5, of 12C2H2 in the 13.7-μm region have been measured. Except for the 2ν2f5 ← ν1f5 hot band, strong intensity perturbation due to l-type doubling and resonances have been quantitatively determined for all hot bands. The zeroth-order vibrational dipole moment derivative for the ν5 bending mode is identical within 10$\%$ for all bands observed. Since an extra factor of 12 appears in the square vibrational transition moment for the combination band, its intensity is reduced by half and is comparable in magnitude to the 2ν5 ← ν5 hot bands. For the ν5 fundamental a square dipole moment derivative of Γ2v = 0.09069(24)D2 has been determined.}
