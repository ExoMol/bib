@article{   71Watson,
   author = {Watson, J.-K.-G.},
    title = {{Forbidden rotational spectra of polyatomic molecules}},
  journal = JMS,
     year = {1971},
   volume = {40},
    pages = {536-544},
    abstract = {Some of the pure rotational transitions of polyatomic molecules
    that are forbidden according to the rigid rotor selection rules may acquire
    intensity by a centrifugal distortion mechanism, in which the intensity
    depends on the dipole derivatives and on the displacements in the normal
    coordinates produced by centrifugal distortion. The structures of the predicted
    spectra are discussed for nonpolar molecules belonging to the point
    groups D3 h, Dn ( n > 2), D2 d, and Td. Some of the calculated R-branch
    lines of methane are stronger than some of the lines of HD already observed
    in the same frequency range. For polar molecules extra contributions to the
    intensity must be considered and are exemplified for the Î” k = Â±3 transitions
    of C3 v molecules. These forbidden rotational transitions may have some
    astrophysical importance.},
doi = {10.1016/0022-2852(71)90255-4},
}

