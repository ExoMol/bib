@ARTICLE{99SkPeBo.methods,
  author = {Skokov, S and Peterson, K A and Bowman, J M},
  title = {{Perturbative inversion of the HOCl potential energy surface via
	singular value decomposition}},
  journal = CPL,
  year = {1999},
  volume = {{312}},
  pages = {494-502},
  abstract = {{A global, ab initio potential energy surface for HOCl {[}K.A. Peterson,
	S. Skokov, J.M. Bowman, J. Chem. Phys. (in press)] is corrected to
	exactly reproduce experimental vibrational energies for 22 known
	bound states, and rotational constants for 9 low-energy states. The
