@article{10ScMaxx,
title = {{On direct product based discrete variable representations for angular coordinates and the treatment of singular terms in the kinetic energy operator}},
journal = CP,
volume = "374",
number = "1",
pages = "118 - 125",
year = "2010",
issn = "0301-0104",
doi = "https://doi.org/10.1016/j.chemphys.2010.07.006",
url = "http://www.sciencedirect.com/science/article/pii/S0301010410003186",
author = "Gerd Schiffel and Uwe Manthe",
keywords = "Quantum dynamics, DVR, FBR, MCTDH, Centrifugal coupling",
abstract = "The use of curvilinear coordinates results in kinetic energy operators with singular terms. These singularities can be in the dynamically relevant region. The present work specifically considers 1/sin2Î¸ singularities and wavefunctions showing significant amplitude close to Î¸=0. It investigates the use of direct product-type discrete variable representations (DVRs) to describe such situations in quantum dynamics calculations and discusses the regularization of such repulsive singularities. A new scheme, the cot-DVR, is developed which can be used to construct well converging direct product DVRs for Hamiltonians showing a 1/sin2Î¸ singularity. The cot-DVR is based on the diagonalization of the cotÎ¸-matrix and adds two additional basis function, sin(Î¸) and sin(2Î¸), to the standard basis of Legendre polynomials. It is particularly well suited for multi-configurational time-dependent Hartree calculations which employ one-dimensional (bottom layer) single-particle functions."
}



@ARTICLE{10MaFaSz.method,
  author = {M\'{a}tyus, Edit and Fabri, Csaba and Szidarovszky, Tamas and Czako,
	Gabor and Allen, Wesley D. and Cs\'{a}sz\'{a}r, Attila G.},
  title = {Assigning quantum labels to variationally computed rotational-vibrational
	eigenstates of polyatomic molecules},
  journal = JCP,
  year = {2010},
  volume = {133},
  pages = {034113},
  abstract = {A procedure is investigated for assigning physically transparent,
	approximate vibrational and rotational quantum labels to variationally
	computed eigenstates. Pure vibrational wave functions are analyzed
	by means of normal-mode decomposition (NMD) tables constructed from
	overlap integrals with respect to separable harmonic oscillator basis
	functions. Complementary rotational labels J(KaKc) are determined
	from rigid-rotor decomposition (RRD) tables formed by projecting
	rotational-vibrational wave functions (J not equal 0) onto products
	of symmetrized rigid-rotor basis functions and previously computed
	(J=0) vibrational eigenstates. Variational results for H(2)O, HNCO,
	trans-HCOD, NCCO, and H(2)CCO are presented to demonstrate the NMD
	and RRD schemes. The NMD analysis highlights several resonances at
	low energies that cause strong mixing and cloud the assignment of
	fundamental vibrations, even in such simple molecules. As the vibrational
	energy increases, the NMD scheme documents and quantifies the breakdown
	of the normal-mode model. The RRD procedure proves effective in providing
	unambiguous rotational assignments for the chosen test molecules
	up to moderate J values.},
  doi = {10.1063/1.3451075}
}

@ARTICLE{10ScLaBe.method,
  author = {Scribano, Yohann and Lauvergnat, David M. and Benoit, David M.},
  title = {Fast vibrational configuration interaction using generalized curvilinear
	coordinates and self-consistent basis},
  journal = JCP,
  year = {2010},
  volume = {133},
  pages = {094103},
  abstract = {In this paper, we couple a numerical kinetic-energy operator approach
	to the direct-vibrational self-consistent field (VSCF)/vibrational
	configuration interaction (VCI) method for the calculation of vibrational
	anharmonic frequencies. By combining this with fast-VSCF, an efficient
	direct evaluation of the ab initio potential-energy surface (PES),
	we introduce a general formalism for the computation of vibrational
	bound states of molecular systems exhibiting large-amplitude motion
	such as methyl-group torsion. We validate our approach on an analytical
	two-dimensional model and apply it to the methanol molecule. We show
	that curvilinear coordinates lead to a significant improvement in
	the VSCF/VCI description of the torsional frequency in methanol,
	even for a simple two-mode coupling expansion of the PES. Moreover,
	we demonstrate that a curvilinear formulation of the fast-VSCF/VCI
	scheme improves its speed by a factor of two and its accuracy by
	a factor of 3.},
  doi = {10.1063/1.3476468}
}

