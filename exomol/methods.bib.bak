  volume = {141},
  pages = {114101},
  abstract = {For computational rovibrational spectroscopy the choice of the frame
	is critical for an approximate separation of overall rotation from
	internal motions. To minimize the coupling between internal coordinates
	and rotation, Eckart proposed a condition {[}{''}Some studies concerning
