@article{ Urban-1994,
Author = {Urban, S and Klee, S and Yamada, K M T},
Title = {GROUND-STATE RO-INVERSIONAL TRANSITIONS OF (NH3)-N-15 IN THE
   FAR-INFRARED REGION},
Journal = JMS,
Year = {1994},
Volume = {168},
Number = {2},
Pages = {384-389},
Month = {DEC},
Abstract = {The high-resolution FIR spectrum of (NH3)-N-15 has been measured in
   natural abundance by the Fourier-transform spectrometer at Giessen,
   Germany in the wavenumber range from 30 to 120 cm(-1). The accurate
   experimental line positions (estimated error of 0.00006 cm(-1)) of the
   ground state rotation-inversion transitions of this isotopomer, which
   have been obtained from a line profile analysis, were fitted by the
   effective rotational Hamiltonian expressed in the conventional
   polynomial way, as well as in the Pade form. The high accuracy of the
   transition wavenumbers has been confirmed by the standard deviations of
   our fits of 0.000040 and 0.000038 cm(-1) obtained with the polynomial
   and Pade forms, respectively. A better extrapolation capability of the
   Pade approximant has been demonstrated. (C) 1994 Academic Press, Inc.},
DOI = {10.1006/jmsp.1994.1287},
ISSN = {0022-2852},
Times-Cited = {7},
Journal-ISO = {J. Mol. Spectrosc.},
Unique-ID = {ISI:A1994PU60800017},
}

@article{ ISI:A1994PN57400011,
Author = {SCHATZ, W and RENK, KF and FUSINA, L and IZATT, JR},
Title = {FAR-INFRARED LASER-EMISSION SPECTROSCOPY ON AMMONIA ISOTOPOMERS},
Journal = {APPLIED PHYSICS B-LASERS AND OPTICS},
Year = {1994},
Volume = {59},
Number = {4},
Pages = {453-465},
Month = {OCT},
Abstract = {In this paper we report the investigation of the isotopic ammonia gases
   (NH3)-N-14, (NH2D)-N-14, (NHD2)-N-14, (NH3)-N-15 and (ND3)-N-15
   optically pumped by a continuously tunable 20 atm CO2-laser. We found
   267 far-infrared emission lines, produced by both inversion and Raman
   processes, in the frequency range from 15 cm(-1) to about 250 cm(-1).
   200 of these were observed for the first time. We obtained far-infrared
   Raman tuning ranges exceeding 3 cm(-1), photon conversion coefficients
   up to 12\% and maximum output energies of 0.9 mJ. Furthermore, the
   optimum gas pressure, the polarization and the transient behavior of
   optically pumped ammonia have been investigated.},
DOI = {10.1007/BF01081068},
ISSN = {0946-2171},
Times-Cited = {9},
Journal-ISO = {Appl. Phys. B-Lasers Opt.},
Unique-ID = {ISI:A1994PN57400011},
}

@article{ Huttner-1992,
Author = {Huttner, W and Frank, U E and Nowicki, P},
Title = {THE (000(0)1(1) VIBRATIONAL G-FACTOR OF AMMONIA, (NH3)-N-15},
Journal = CPL,
Year = {1992},
Volume = {196},
Number = {6},
Pages = {614-618},
Month = {AUG 28},
Abstract = {Zeeman splittings of microwave l-type inversional transitions in the
   nu-4 = (000(0)1(1)) degenerate vibrational mode of (NH3)-N-15 have been
   measured and analyzed. Calling p(z) the pure vibrational angular
   momentum along the figure axis and defining the vibrational g factor by
   the corresponding magnetic dipole component, mu(N)g4p(z), we find g4 =
   0.95(3) (mu(N) = nuclear magneton). The rotational g factors of the
   frame, g(perpendicular-to) = 1/2 (g(xx) + g(yy)) = 0.5609(7) and
   g(parallel-to) = g(zz) = 0.5081(6), are remarkably close to those of the
   ground state. Differences in the vibrational Zeeman effects of linear
   and symmetric-top molecules are discussed.},
DOI = {10.1016/0009-2614(92)86003-Z},
ISSN = {0009-2614},
Times-Cited = {10},
Journal-ISO = {Chem. Phys. Lett.},
Unique-ID = {ISI:A1992JK19200016},
}

@article{ Moriwaki-1991,
Author = {Moriwaki, Y and Nakagawa, K and Shimizu, T},
Title = {OBSERVATION AND ANALYSIS OF THE DELTA-UPSILON=5 STRETCH OVERTONE BAND
   TRANSITION OF (NH3)-N-15},
