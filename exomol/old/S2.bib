  year = {1997},
  volume = {93},
  pages = {365-372},
  abstract = {Spectra and lifetime measurements of the strongly mixed B `'(3) Pi(u)
	and B-3 Sigma(u)(-) states of S-32(2) are presented covering v' =7-10
	of the B state and v' =12-21 of the B `' state. This extends our
