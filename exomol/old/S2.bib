  journal = CP,
  year = {1992},
  volume = {166},
  pages = {229-239},
  abstract = {In this work the S2 molecule has been studied using resonance enhanced
	multiphoton ionization (MPI) spectroscopy in the single-photon wavelength
	region 285-320 nm. This has resulted in the first spectroscopic observation
	of a gerade Rydberg state of S2. The observed bands, which show spin-orbit
	structure, have been assigned to a two-photon resonant process, 3DELTA(g)(3dpi)
	<-- X3SIGMA(g)-. Transitions to three vibrational levels (v' = 0,
	1, 2) of the excited state have been recorded. This has enabled determination
	of vibrational constants (omega(e)BAR), and omega(e)x(e)BAR) and
	the spin-orbit coupling constant for the 3DELTA(g) state, whilst
	rotational constants (B(v)) were derived from band head separations.
	A band in the MPI SpeCtruM Of S2 corresponding to the one-photon
	resonance B3SIGMA(u)-, v' = 8 <-- X3SIGMA(g)-, v{''} = 0 has also
	been observed. This has been detected in both the S2+ and S+ mass
	channels. Possible mechanisms for production of these ions via the
	B3SIGMA(u)- state are discussed.},
  doi = {10.1016/0301-0104(92)87021-Z},
  keywords = {Experiment}
}

@ARTICLE{93BrSmxx.S2,
