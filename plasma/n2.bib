@article{mul,
  author={P M Mul and J W McGowan},
  title={Merged electron-ion beam experiments. III. Temperature dependence of dissociative recombination for atmospheric ions NO + , O 2 + and N 2 +},
  journal={J. Phys.B: At. Mol. Phys.},
  volume={12},
  pages={1591},
  year={1979},
  abstract={Electron-molecular-ion dissociative recombination cross sections have been measured for N 2 + , NO + and O 2 + over the energy interval 0.007-1 eV using the merged electron-ion beam technique. As with most other molecular ions measured by us the 'direct' recombination process appears to dominate below 0.1 eV where all cross sections follow a 1/E dependence. For the ions O 2 + , NO + and N 2 + the derived rate coefficients at 300K are respectively 1.9, 2.3 and 3.5*10 -7 cm 3 s -1 with a temperature dependence close to T c -0.5 .}
}
	



