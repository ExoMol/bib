@article{ gk06,
Author = {Greene, Chris H. and Kokoouline, V.},
title = {{Theoretical progress and challenges in H-3(+) dissociative recombination}},
journal = {Phil.  Trans.  Royal Soc.  London A},
Year = {{2006}},
Volume = {{364}},
Pages = {2965-2980},
Abstract = {{We discuss the Jahn-Teller mechanism for dissociative recombination in
   low energy collisions between electrons and H-3(+) ions, in energy
   ranges relevant to the processes underway in interstellar clouds. While
   theory has become capable of predicting recombination rates in
   reasonable agreement with storage ring experiments, some discrepancies
   remain with them, and a long-standing discrepancy with stationary
   afterglow measurements remains troubling. Speculations about the
   desirable improvements in both theory and experiment are presented.}}}

